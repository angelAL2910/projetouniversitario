@model IEnumerable<KSI.Cobranza.Web.Models.ViewModels.QueueViewModels>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="stcontent-body">
    <div class="modal fade" id="ModalMovimiento" tabindex="-1" role="dialog" aria-labelledby="ModalMovimientoLabel">
    </div>
    <div id="myModal" class="modal fade" tabindex="-1" role="dialog">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                    <h4 id="titleModal" class="modal-title"></h4>
                </div>
                <div id="bodymodal" class="modal-body">
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">
            <div class="row">
                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                </div>
                <div class="col-lg-2 col-md-2 col-sm-12 col-xs-12">
                </div>
                <div class="col-lg-2 col-md-2 col-sm-12 col-xs-12">
                    <div class="btn-group" role="group" aria-label="...">
                        <button type="button" class="btn btn-secundary-st " disabled="" id="btnQueueAssignCase" data-toggle="modal"
                                data-target="#ModalQueueCaseAssign">
                            <i class="fa fa-user-plus" aria-hidden="true"></i>
                            Asignar casos
                        </button>
                        <button type="button" class="btn btn-secundary-st" disabled="" id="btnQueueAssignNote" data-toggle="modal" data-target="#ModalQueueAssignNote">
                            <i class="fa fa-commenting-o" aria-hidden="true"></i>
                            Agregar nota
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="contenedor_tabs">
        <ul class="tabs_ttle dvddo_in_3" id="UlMainMenu">
            <li class="active dos_lineas" onclick="SetStage(1, this);">
                <span class="num_cases">4</span>
                <a href="#">
                    <span>
                        PROMESAS DEL DÍA
                    </span>
                </a>
            </li>
            <li class="dos_lineas" onclick="SetStage(2, this);">
                <span class="num_cases">1</span>
                <a href="#">
                    <span>
                        COBRANZAS CORRECTIVA
                    </span>
                </a>
            </li>
            <li class=" dos_lineas" onclick="SetStage(3,this);">
                <span class="num_cases">1</span>
                <a href="#">
                    <span>
                        COBRANZAS JUDICIAL
                    </span>
                </a>
            </li>
        </ul>
        <div id="dvMenues">
            <div id="dvPromesasDelDia">

            </div>
            <div id="dvCobranzasCorrectivas" style="display:none">
                <ul class="atlantica_steps kscobranzas">
                    <li class="active dos_lineas" role="presentation" style="">
                        <span class="num_cases2">3</span>
                        <a href="#" onclick="javascript: SetStatus(20); return false;" class="una_linea">
                            Atrasos 0-5 días
                        </a>
                    </li>
                    <li class=" dos_lineas" role="presentation" style="">
                        <span class="num_cases2">1</span>
                        <a href="#" onclick="javascript: SetStatus(25); return false;" class="una_linea">
                            Atrasos 6-16 días
                        </a>
                    </li>
                    <li class=" dos_lineas" role="presentation" style="">
                        <span class="num_cases2">0</span>
                        <a href="#" onclick="javascript: SetStatus(26); return false;" class="una_linea">
                            Atrasos 16-21 días
                        </a>
                    </li>
                    <li class=" dos_lineas" role="presentation" style="">
                        <span class="num_cases2">0</span>
                        <a href="#" onclick="javascript: SetStatus(26); return false;" class="una_linea">
                            Cobranza Legal
                        </a>
                    </li>
                </ul>
            </div>
            <div id="dvCobranzasJudiales" style="display:none">
                <ul class="atlantica_steps kscobranzas">
                    <li class="active dos_lineas" role="presentation" style="">
                        <span class="num_cases2">3</span>
                        <a href="#" onclick="javascript: SetStatus(20); return false;" class="dos_linea">
                            MANDAMIENTO DE PAGO
                        </a>
                    </li>
                    <li class=" dos_lineas" role="presentation" style="">
                        <span class="num_cases2">1</span>
                        <a href="#" onclick="javascript: SetStatus(25); return false;" class="dos_linea">
                            REQUERIMIENTO DE PRENDA
                        </a>
                    </li>
                    <li class=" dos_lineas" role="presentation" style="">
                        <span class="num_cases2">0</span>
                        <a href="#" onclick="javascript: SetStatus(26); return false;" class="dos_linea">
                            INCAUTACIÓN DEL VEHÍCULO
                        </a>
                    </li>
                    <li class=" dos_lineas" role="presentation" style="">
                        <span class="num_cases2">0</span>
                        <a href="#" onclick="javascript: SetStatus(26); return false;" class="una_linea">
                            AUDIENCIA DE VENTA
                        </a>
                    </li>
                    <li class=" dos_lineas" role="presentation" style="">
                        <span class="num_cases2">1</span>
                        <a href="#" onclick="javascript: SetStatus(25); return false;" class="dos_linea">
                            SENTENCIA DE ADJUDICACIÓN
                        </a>
                    </li>
                    <li class=" dos_lineas" role="presentation" style="">
                        <span class="num_cases2">0</span>
                        <a href="#" onclick="javascript: SetStatus(26); return false;" class="una_linea">
                            COBRANZA LEGAL
                        </a>
                    </li>
                </ul>

            </div>
        </div>
        <div class="clear"></div>
    </div>
    <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
        <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="headingOne">
                <h4 class="panel-title">
                    <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                        <i class="fa fa-book" aria-hidden="true"></i>
                        DATOS GENERALES DE LA BANDEJA
                    </a>
                </h4>
            </div>

            <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                <div class="panel-body">
                    <!--
                    Cuerpo del Queue
                    -->
                    <div class="table-responsive">
                        <table class="table table-striped qcobranzas">
                            <tbody>
                                <tr>
                                    <td>No. Préstamo</td>
                                    <td>Id</td>
                                    <td>Nombre del Cliente</td>
                                    <td>Monto financiado</td>
                                    <td>Tasa de Interés</td>
                                    <td>Ejecutivo</td>
                                    <td>Promotor</td>
                                    <td>Plazo</td>
                                    <td>Estado del crédito</td>
                                    <td>Oficina</td>
                                    <td>Producto</td>
                                    <td>Puesto en QUEUE</td>
                                    <td>Fecha de acción</td>
                                    <td>Departamento que refiere</td>
                                    <td>Asignado a</td>
                                    <td>Tipo de Seguimiento</td>
                                    <td>Llamar</td>
                                    <td>Monto adeudado</td>
                                    <td>Cobrar</td>
                                </tr>

                                @foreach (var item in Model)
                                {
                                    <tr onclick="goToProcess('@item.LoanNumber')">
                                        <td>@item.LoanNumber</td>
                                        <td>@item.Id</td>
                                        <td>@item.CustomerFullName</td>
                                        <td>@item.AmountFinanced</td>
                                        <td>@item.InterestRate</td>
                                        <td>@item.ExecutiveName</td>
                                        <td>@item.SponsorName</td>
                                        <td>@item.Term</td>
                                        <td>@item.CreditState</td>
                                        <td>@item.Office</td>
                                        <td>@item.Product</td>
                                        <td>@item.QueuePosition</td>
                                        <td>@item.ActionDate</td>
                                        <td>@item.ReferrerDepartment</td>
                                        <td>@item.AssignTo</td>
                                        <td>@item.KindOfFollowup</td>
                                        <td>@item.ToCall</td>
                                        <td>@item.AmountOwed</td>
                                    </tr>
                                }

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="moduloNotesAtlantica">
        <div class="moduloNotesAtlanticaHeader">
            <label class="titulomoduloNotesAtlantica">
                <i class="fa fa-commenting-o" aria-hidden="true"></i>
                Escribir una nota
            </label>
            <a id="btnMinimizarmoduloNotesAtlantica" href="#" class="btnMinimizarmoduloNotesAtlantica" onclick="minizarModalSTSV()" style="display: none;"></a>
            <a id="btnMaximizarmoduloNotesAtlantica" href="#" class="btnMaximizarmoduloNotesAtlantica" onclick="maximizarModalSTSV()" style="display: none;"></a>
        </div>
    </div>
</div>
<link href="~/Content/atlantica.css" rel="stylesheet" />
<script src="~/Scripts/ViewJS/Queue.js"></script>


