@model  IEnumerable<KSI.Cobranza.Web.Models.ViewModels.BaseViewModels.KeyValue>
@{
    var webGrid = new WebGrid(source: Model, ajaxUpdateContainerId: "dvGridGuaranteeDetail", canPage: false);
}
<div id="dvGridGuaranteeDetail">
    @webGrid.GetHtml(tableStyle: "table table-striped car PhoneMail",
                                        headerStyle: "header-table",
                                        htmlAttributes: new { @id = "gvGuaranteesVehiclesDetail" },
                                        columns: webGrid.Columns(webGrid.Column("Key", null), webGrid.Column("Value", null)
                                   )
                                )
</div>