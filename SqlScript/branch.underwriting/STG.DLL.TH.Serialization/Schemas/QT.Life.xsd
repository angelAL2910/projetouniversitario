<?xml version="1.0" encoding="ISO-8859-1"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:element name="SYSTEM_OIPA">
	<xs:complexType>
			<xs:sequence>
			<xs:element name="dataset" type="SYSTEM_OIPA" maxOccurs="unbounded"/>			
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="SYSTEM_OIPA">
		<xs:sequence>
			<xs:element name="PolicyInfo" type="PolicyInfo" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Members" type="Member" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Recipients" type="Recipient" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Appointment" type="Appointment" minOccurs="0" maxOccurs="1"/>
			<xs:element name="PaymentDetail" type="PaymentDetail" minOccurs="0" maxOccurs="1"/>	
			<xs:element name="PrimeResume" type="PrimeResume" minOccurs="0" maxOccurs="1"/>			
			<xs:element name="Coverages" type="Coverage" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Additionals" type="Additional" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="CollectivePolicy" type="CollectivePolicy" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Collections" type="Collection" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="AmountInsuredArray" type="AmountInsuredArray" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="AmountInsuredTotal" type="AmountInsuredTotal" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Quotation" type="Quotation" minOccurs="0" maxOccurs="1"/>
			<xs:element name="DynamicAttachment" type="DynamicAttachment" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Categoria" type="Categoria" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PolicyData" type="PolicyData" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>	
	</xs:complexType>

	<xs:complexType name="PolicyInfo">
		<xs:sequence>
			<!-- Thunderhead Document ID-->
			<xs:element type="xs:long" name="DocumentId" minOccurs="1"/>
			<!-- Insured Full Name Policy Owner-->
			<xs:element type="xs:string" name="PrimaryInsuredName" minOccurs="0" maxOccurs="1"/>			
			<!-- Name prefix i.e. Mr. Miss. Mrs., etc-->
			<xs:element type="xs:string" name="PrefixInsuredName" minOccurs="0" maxOccurs="1"/>			
			<!-- Policy Number -->
			<xs:element type="xs:string" name="PolicyNumber" minOccurs="1" maxOccurs="1"/>
			<xs:element type="xs:string" name="Intermediary" minOccurs="0" maxOccurs="1"/>
		    <!--value: Life Insurance-->
			<xs:element type="xs:string" name="Branch" minOccurs="0" maxOccurs="1"/>		
			<xs:element type="xs:decimal" name="RescueValue" minOccurs="0" maxOccurs="1"/>			
			<!--Orion, Luminis, Guardian, etc-->
			<xs:element type="xs:string" name="PolicyType" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="Plan" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="Product" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="Address" minOccurs="0" maxOccurs="1"></xs:element>			
			<xs:element type="xs:date" name="PolicyPeriodStart" minOccurs="0" maxOccurs="1"/>			
			<xs:element type="xs:date" name="PolicyPeriodEnd" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:date" name="PolicyPeriodEndFee" minOccurs="0" maxOccurs="1"/>				
			<xs:element type="xs:date" name="PolicyCancellationDate" minOccurs="0" maxOccurs="1"/>			
			<!-- Policy Current Status -->
			<xs:element type="xs:string" name="PolicyStatus" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:integer" name="Temporality" minOccurs="0" maxOccurs="1"/>		
			<xs:element type="xs:integer" name="LivesInsuredNo" minOccurs="0" maxOccurs="1"/>
			<!-- Cantidad de Vidas Aseguradas-->
			<xs:element type="xs:string" name="User" minOccurs="0" maxOccurs="1"/>
			<!-- Nombre de Usuario que emite poliza/oferta -->
			<xs:element type="xs:string" name="Code" minOccurs="0" maxOccurs="1"/>
			<!-- Codigo del Asesor Comercial -->
			<xs:element type="xs:integer" name="InsuranceDuration" minOccurs="0" maxOccurs="1"/>
			<!-- Periodo duracion del seguro -->
			<xs:element type="xs:date" name="EffectiveDateAmendment" minOccurs="0" maxOccurs="1"/>
			<!-- Fecha de Vigencia de la Enmienda -->
			<xs:element type="xs:string" name="Declarations" minOccurs="0" maxOccurs="1"/>	
			<!-- Declaraciones del Propietario o Asegurado -->
			<xs:element type="xs:string" name="PrincipalInsuredReq" minOccurs="0" maxOccurs="1"/>	
			<!-- Requisitos Asegurado Principal -->
			<xs:element type="xs:string" name="AdditionalInsuredReq" minOccurs="0" maxOccurs="1"/>	
			<!-- Requisitos Asegurado Adicional -->
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Member">
		<xs:sequence>
			<!--MemberID-->
			<xs:element type="xs:string" name="memberId" minOccurs="1"/>
			<!-- Primary Beneficiary, Contingent Beneficiary, Insured, Owner, Writing Agent-->
			<xs:element type="xs:integer" name="Type" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="FirstName" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="MiddleName" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="LastName" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="SecondLastName" minOccurs="0" maxOccurs="1"/>
			<!-- Name prefix i.e. Mr. Miss. Mrs., etc-->
			<xs:element type="xs:string" name="Prefix" minOccurs="0" maxOccurs="1"/>
			<!-- Nombre completo de Miembro de la poliza-->
			<xs:element type="xs:string" name="memberFullName" minOccurs="0" maxOccurs="1"/>
			<!--Primary beneficiary | Secondary beneficiary -->			
			<xs:element type="xs:string" name="FamiliarityWithOwner" minOccurs="0" maxOccurs="1"></xs:element>			
			<xs:element type="xs:integer" name="Dependents" minOccurs="0" maxOccurs="1"></xs:element>		
			<xs:element type="xs:date" name="dateOfBirth" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:integer" name="Age" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="RequiredMedicalExam" minOccurs="0" maxOccurs="1"/>			
			<xs:element type="xs:string" name="Sex" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element type="xs:string" name="Country" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element type="xs:string" name="City" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element type="xs:string" name="Address" minOccurs="0" maxOccurs="1"></xs:element>			
			<xs:element type="xs:string" name="CivilStatus" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element type="xs:integer" name="Smoker" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element type="xs:string" name="CompanyName" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element type="xs:string" name="Role" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element type="xs:integer" name="Percentage" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element type="xs:string" name="NIT" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element type="xs:string" name="Risk" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element type="xs:string" name="Millar" minOccurs="0" maxOccurs="1"></xs:element>
    </xs:sequence>
	</xs:complexType>
  
	<xs:complexType name="Recipient">
		<xs:sequence>
			<!--numero de reclamacion-->
			<xs:element type="xs:string" name="UserEmail" minOccurs="1" maxOccurs="1"/>
			<xs:element type="xs:string" name="CCopy" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="BCCopy" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="PhoneNumber" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:integer" name="SendByPhone" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Appointment">
		<xs:sequence>
			<xs:element type="xs:date" name="dateOfMedicalAppointment" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="Date" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="Time" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="PhoneNumber" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="Comment" minOccurs="0" maxOccurs="1"/>
		    <xs:element type="xs:string" name="MedicalAppointmentStatus" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="PaymentDetail">
		<xs:sequence>
			<xs:element type="xs:string" name="MethodOfPayment" minOccurs="0"  maxOccurs="1"/>
			<xs:element type="xs:string" name="FrequencyOfPayment" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:date" name="dateOfPayment" minOccurs="0" maxOccurs="1"/>
		    <xs:element type="xs:decimal" name="AmountOfPayment" minOccurs="0" maxOccurs="1"/>
		    <xs:element type="xs:string" name="CheckNumber" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:date" name="Returndate" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:decimal" name="ReturnOfPremium" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="CreditCardNumber" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="Approval" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="AccountNumber" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="ReceivingBank" minOccurs="0" maxOccurs="1"/>		
			<xs:element type="xs:string" name="ReceivingAccountNumber" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="IssuingBank" minOccurs="0" maxOccurs="1"/>
			<!-- Nombre del Banco Emisor -->
			<xs:element type="xs:date" name="IssueDate" minOccurs="0" maxOccurs="1"/>
			<!-- Fecha de Emisión -->
			<xs:element type="xs:date" name="GracePeriodDate" minOccurs="0" maxOccurs="1"/>
			<!-- Fecha período de gracia por falta de pago -->
			<xs:element type="xs:decimal" name="AnnualContributionAmount" minOccurs="0"  maxOccurs="1"/>
			<xs:element type="xs:string" name="ModeOfContribution" minOccurs="0"  maxOccurs="1"/>
			<xs:element type="xs:string" name="Currency" minOccurs="0"  maxOccurs="1"/>
			<xs:element type="xs:integer" name="InitialContribution" minOccurs="0"  maxOccurs="1"/>
			<xs:element type="xs:decimal" name="AdditionalAmount" minOccurs="0"  maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="PrimeResume">
		<xs:sequence>
            <xs:element type="xs:decimal" name="TotalAnualPrime" minOccurs ="0" maxOccurs ="1"/>
            <!-- Total Prima Anual -->
            <xs:element type="xs:decimal" name="Taxes" minOccurs ="0" maxOccurs ="1"/>
            <!-- Impuestos -->
            <xs:element type="xs:decimal" name="Discount" minOccurs ="0" maxOccurs ="1"/>
            <!-- Descuentos - no esta en el doc enviado pero lo envio igual -->
            <xs:element type="xs:decimal" name="TotalPayment" minOccurs ="0" maxOccurs ="1"/>
            <!-- Total a pagar-->
			<xs:element type="xs:decimal" name="CurrentPrime" minOccurs ="0" maxOccurs ="1"/>
            <!-- Prima Actual-->
			<xs:element type="xs:decimal" name="MinAnnualPrime" minOccurs ="0" maxOccurs ="1"/>
			<!-- Prima Minima Anual-->
			<xs:element type="xs:decimal" name="MinMonthlyPrime" minOccurs ="0" maxOccurs ="1"/>
            <!-- Prima Minima Mensual-->
			<xs:element type="xs:decimal" name="IndividualAnualPrime" minOccurs ="0" maxOccurs ="1"/>
            <!-- Prima Annual Individual-->
			<xs:element type="xs:decimal" name="PeriodicalPrime" minOccurs ="0" maxOccurs ="1"/>
			<!-- Modo de Aporte: Prima Periodica -->
			<xs:element type="xs:decimal" name="InicialPrime" minOccurs ="0" maxOccurs ="1"/>
			<!-- Prima Inicial: Moneda del plan  -->
			<xs:element type="xs:decimal" name="InsuranceAmount" minOccurs ="0" maxOccurs ="1"/>
			<!-- Suma Asegurada Inicial: "monto del seguro" -->
			<xs:element type="xs:integer" name="ContributionPeriod" minOccurs ="0" maxOccurs ="1"/>
			<!-- Período Esperado de Contribución: « »Años de contribución" -->
			<xs:element type="xs:string" name="InvestmentProfile" minOccurs ="0" maxOccurs ="1"/>
			<!-- Perfil de Inversión Inicial -->
			<xs:element type="xs:decimal" name="AnnualPremium" minOccurs ="0" maxOccurs ="1"/>
			<!-- Prima Planeada -->
			<xs:element type="xs:decimal" name="InicialPrimeTaxes" minOccurs ="0" maxOccurs ="1"/>
			<!-- Impuestos Prima Inicial -->
			<xs:element type="xs:decimal" name="PeriodicalPrimeTaxes" minOccurs ="0" maxOccurs ="1"/>
			<!-- Impuestos Prima Periodica-->
			<xs:element type="xs:decimal" name="PremiumReturnAmount" minOccurs ="0" maxOccurs ="1"/>
			<!-- Monto Devolucion de Prima-->
			<xs:element type="xs:decimal" name="TotalPeriodicalPrime" minOccurs ="0" maxOccurs ="1"/>
			<!-- Prima Periodica Total -->
			<xs:element type="xs:decimal" name="TotalInsuredAmount" minOccurs ="0" maxOccurs ="1"/>
			<!-- Suma Asegurada Total -->
			<xs:element type="xs:decimal" name="BaseInsuredAmount" minOccurs ="0" maxOccurs ="1"/>
			<!-- Suma Asegurada Base -->
			<xs:element type="xs:decimal" name="TargetPrime" minOccurs ="0" maxOccurs ="1"/>
			<!-- Prima Target -->
        </xs:sequence>	
	</xs:complexType>
	
	<!-- Cotizacion -->
	<xs:complexType name="Quotation">
		<xs:sequence>
			<xs:element type="xs:string" name="QuotationNumber" minOccurs="0"  maxOccurs="1"/>	
			<xs:element type="xs:date" name="ProposalDate" minOccurs="0"  maxOccurs="1"/>
			<xs:element type="xs:date" name="EndOfProposalDate" minOccurs="0"  maxOccurs="1"/>	
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="Coverage">
		<xs:sequence>
			<xs:element type="xs:string" name="Description" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:decimal" name="AmountInsured" minOccurs ="0" maxOccurs ="1"/>	
			<xs:element type="xs:integer" name="TimeCoverage" minOccurs ="0" maxOccurs ="1"/> <!-- Tiempo Cobertura en Años-->
			<xs:element type="xs:string" name="Debtor" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:decimal" name="MaximumInsured" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:decimal" name="MonthlyFee" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:decimal" name="AmountOfCoverage" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:integer" name="CoverageAge" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:decimal" name="Limit" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:string" name="Category" minOccurs="0"  maxOccurs="1"/>
			<!-- Categoria-->
			<xs:element type="xs:string" name="TypeInsuredAmount" minOccurs="0"  maxOccurs="1"/>
			<!-- Tipo de Suma Asegurada-->
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="Additional"> 
		<xs:sequence>
			<xs:element type="xs:string" name="Description" minOccurs ="0" maxOccurs ="1"/>	
			<xs:element type="xs:decimal" name="Limit" minOccurs ="0" maxOccurs ="1"/>	
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="CollectivePolicy">
		<xs:sequence>
			<xs:element type="xs:string" name="memberId" minOccurs="0"  maxOccurs="1"/>
			<xs:element type="xs:string" name="CertificateNumber" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:string" name="InsuredName" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:string" name="InsuredAddress" minOccurs="0"  maxOccurs="1"/>
			<xs:element type="xs:integer" name="Age" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:date" name="InspectionDate" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:decimal" name="AmountInsured" minOccurs ="0" maxOccurs ="1"/>	
			<xs:element type="xs:decimal" name="Prime" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:string" name="BeneficiaryNumber" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="Beneficiary" name="Beneficiary" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element type="Coverage" name="Coverages" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element type="xs:date" name="dateOfBirth" minOccurs="0"  maxOccurs="1"/>
			<xs:element type="xs:date" name="ExclusionDate" minOccurs="0"  maxOccurs="1"/>
			<!-- Fecha de Exclusion-->
			<xs:element type="xs:date" name="InclusionDate" minOccurs="0"  maxOccurs="1"/>
			<!-- Fecha de Inclusion-->
			<xs:element type="xs:string" name="Motive" minOccurs="0"  maxOccurs="1"/>
			<!-- Motivo de Exclusion-->
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="Beneficiary">
		<xs:sequence>
			<xs:element type="xs:string" name="BeneficiaryName" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:date" name="DateOfBirth" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:string" name="Relationship" minOccurs ="0" maxOccurs ="1"/>
			<xs:element type="xs:string" name="Assignment" minOccurs ="0" maxOccurs ="1"/>		
		</xs:sequence>
	</xs:complexType>
	
	<!-- Cobros -->
	<xs:complexType name="Collection">
		<xs:sequence>
			<xs:element type="xs:integer" name="QuotaNumber" minOccurs="0"  maxOccurs="1"/>
			<!-- Numero de Cuota -->
			<xs:element type="xs:string" name="RegistryNumber" minOccurs="0"  maxOccurs="1"/>
			<!-- Registro -->
			<xs:element type="xs:string" name="InvoiceNumber" minOccurs="0"  maxOccurs="1"/>
			<!-- Cause Numero -->
			<xs:element type="xs:decimal" name="FractionationSurcharge" minOccurs="0"  maxOccurs="1"/>
			<!-- Recargo por Fraccionamiento -->
			<xs:element type="xs:decimal" name="NetPrime" minOccurs="0"  maxOccurs="1"/>
			<!-- Prima neta -->
			<xs:element type="xs:date" name="DueDate" minOccurs="0"  maxOccurs="1"/>
			<!-- Fecha de Vencimiento -->
		</xs:sequence>
	</xs:complexType>
	
		<!-- Sumas Aseguradas - Detalle -->
	<xs:complexType name="AmountInsuredArray">
		<xs:sequence>
			<xs:element type="xs:string" name="Category" minOccurs="0"  maxOccurs="1"/>
			<!-- CategorÃ­a -->
			<xs:element type="xs:integer" name="EmployeesNo" minOccurs="0"  maxOccurs="1"/>
			<!-- NÂ° de Empleados -->
			<xs:element type="xs:decimal" name="AmountInsuredPerEmployee" minOccurs="0"  maxOccurs="1"/>
			<!-- Suma Asegurada por Empleado -->
			<xs:element type="xs:decimal" name="Rate" minOccurs="0"  maxOccurs="1"/>
			<!-- Tarifa al millar -->
			<xs:element type="xs:decimal" name="TotalAnualPrime" minOccurs="0"  maxOccurs="1"/>
			<!-- Prima Total Anual -->
		</xs:sequence>
	</xs:complexType>
	
	<!-- Sumas Aseguradas - Totales -->
	<xs:complexType name="AmountInsuredTotal">
		<xs:sequence>
			<xs:element type="xs:integer" name="EmployeesNo" minOccurs="0"  maxOccurs="1"/>
			<!-- Total NÂ° de Empleados -->
			<xs:element type="xs:decimal" name="AmountInsuredPerEmployee" minOccurs="0"  maxOccurs="1"/>
			<!-- Total Suma Asegurada por Empleado -->
			<xs:element type="xs:decimal" name="Rate" minOccurs="0"  maxOccurs="1"/>
			<!-- Total Tarifa al millar -->
			<xs:element type="xs:decimal" name="TotalAnualPrime" minOccurs="0"  maxOccurs="1"/>
			<!-- Total Prima Total Anual -->
		</xs:sequence>
	</xs:complexType>	
	
	<!-- Categorias Coberturas -->
	<xs:complexType name="Categoria">
		<xs:sequence>
			<xs:element type="xs:string" name="Description" minOccurs="0"  maxOccurs="1"/>
			<!-- Descripcion o Nombre de La Categoria-->
			<xs:element type="Coverage" name="Coverages" minOccurs="0" maxOccurs="unbounded"/>
			<!-- Coberturas de la Categoria-->
		</xs:sequence>
	</xs:complexType>
	
	
	<xs:complexType name="DynamicAttachment">
		<xs:sequence>
			<!--Added 07/07/2017-->
			<xs:element type="xs:string" name="Document1" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="Path1" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="Document2" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="Path2" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="PolicyData">
		<xs:sequence>
			<xs:element type="xs:integer" name="Age" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:integer" name="YearsOfThePlan" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:decimal" name="BasicPrimeCoverage" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:decimal" name="SupplementsPrime" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:decimal" name="TotalPrime" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:decimal" name="AccumulatedPremiums" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="DeathBenefit" minOccurs="0" maxOccurs="1"/>
			<xs:element type="xs:string" name="TipoPrima" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	
</xs:schema>