/*==|=====================|=\
	RADIO BUTTON STATETRUST
===========================*/

.label {
    margin: 5px 0;
    text-transform: none;
    min-height: 7px;
    line-height: 15px;
    font-family: Arial, Helvetica, sans-serif;
    display: inline-block;
    font-weight: bold;
    color: #111;
    font-size: 12px;
    display: inline-block;
    text-indent: 2px;
}

.radio label {
    display: inline-block;
    cursor: pointer;
    position: relative;
    padding-left: 35px;
    font-size: 14px;
    line-height: 14px;
    background: none !important;
}

.radio input[type=radio] {
    display: none;
}

.radio label:before {
    content: "";
    color: #aaa;
    display: inline-block;
    width: 25px;
    height: 25px;
    text-align: center;
    line-height: 12px;
    font-size: 20px;
    margin-right: 10px;
    position: absolute;
    left: 2px;
    bottom: -11px;
    background: #00b2fc;
    background: url(data:image/svg+xml;base64,PD94bWwgdmVyc2lvbj0iMS4wIiA/Pgo8c3ZnIHhtbG5zPSJodâ€¦EiIGhlaWdodD0iMSIgZmlsbD0idXJsKCNncmFkLXVjZ2ctZ2VuZXJhdGVkKSIgLz4KPC9zdmc+);
    background: -moz-linear-gradient(top, #00b2fc 0%, #02577b 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#00b2fc), color-stop(100%,#02577b));
    background: -webkit-linear-gradient(top, #00b2fc 0%,#02577b 100%);
    background: -o-linear-gradient(top, #00b2fc 0%,#02577b 100%);
    background: -ms-linear-gradient(top, #00b2fc 0%,#02577b 100%);
    background: linear-gradient(to bottom, #00b2fc 0%,#02577b 100%);
}

.radio label:before {
    border-radius: 25px;
}

.radio label:after {
    content: "";
    width: 15px;
    height: 15px;
    border-radius: 20px;
    background: #fff;
    position: absolute;
    left: 7px;
    top: -3px;
}

.radio input[type=radio]:checked + label:after {
    content: "\2022";
    color: #63bd0f;
    font-size: 32px;
    line-height: 16px;
}

.swich_enableR {
    content: "\2022";
    color: #63bd0f;
    font-size: 32px;
    line-height: 16px !important;
}

.swich_disableR {
    color: #63bd0f;
    font-size: 32px;
    line-height: 16px !important;
}

.radioSelect:before {
    content: "\2022" !important;
    color: #63bd0f !important;
    font-size: 20px !important;
    text-align: center !important;
    border: 4px solid #63bd0f !important;
    line-height: 12px !important;
    display: inline-block !important;
    width: 20px;
    height: 20px;
    margin-right: 10px;
    position: absolute;
    left: 0;
    bottom: -3px;
    background-color: #fff;
}
/*-------------| RADIOCHECK |--------------*/

.radiocheck_Cont input[type=checkbox] {
    display: none;
}

.radiocheck {
    padding-left: 30px;
    position: relative;
    cursor: pointer;
}

    .radiocheck:before {
        -webkit-appearance: none;
        padding: 9px;
        border-radius: 25px;
        display: inline-block;
        position: relative;
        cursor: pointer;
        content: "";
        position: absolute;
        left: 0px;
        top: -6px;
        width: 25px;
        height: 25px;
        background: #00b2fc;
        background: url(data:image/svg+xml;base64,PD94bWwgdmVyc2lvbj0iMS4wIiA/Pgo8c3ZnIHhtbG5zPSJodâ€¦EiIGhlaWdodD0iMSIgZmlsbD0idXJsKCNncmFkLXVjZ2ctZ2VuZXJhdGVkKSIgLz4KPC9zdmc+);
        background: -moz-linear-gradient(top, #00b2fc 0%, #02577b 100%);
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#00b2fc), color-stop(100%,#02577b));
        background: -webkit-linear-gradient(top, #00b2fc 0%,#02577b 100%);
        background: -o-linear-gradient(top, #00b2fc 0%,#02577b 100%);
        background: -ms-linear-gradient(top, #00b2fc 0%,#02577b 100%);
        background: linear-gradient(to bottom, #00b2fc 0%,#02577b 100%);
    }

    .radiocheck:after {
        content: '';
        border-radius: 20px;
        width: 15px;
        height: 15px;
        position: absolute;
        top: 5px;
        left: 5px;
        background-color: #fff;
        position: absolute;
        left: 5px;
        top: -1px;
        cursor: pointer;
    }

.radiocheck_Cont input[type=checkbox]:checked + .radiocheck:after {
    content: '\2022';
    font-size: 32px;
    color: #63bd0f;
    line-height: 17px;
}

.radiocheckSelect:after {
    content: '\2022';
    font-size: 32px;
    color: #63bd0f;
    line-height: 17px;
}

.disableCheck .radiocheck:before {
    background: #a9a9a9;
    background: url(data:image/svg+xml;base64,PD94bWwgdmVyc2lvbj0iMS4wIiA/Pgo8c3ZnIHhtbG5zPSJodâ€¦EiIGhlaWdodD0iMSIgZmlsbD0idXJsKCNncmFkLXVjZ2ctZ2VuZXJhdGVkKSIgLz4KPC9zdmc+);
    background: -moz-linear-gradient(top, #a9a9a9 0%, #898989 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#a9a9a9), color-stop(100%,#898989));
    background: -webkit-linear-gradient(top, #a9a9a9 0%,#898989 100%);
    background: -o-linear-gradient(top, #a9a9a9 0%,#898989 100%);
    background: -ms-linear-gradient(top, #a9a9a9 0%,#898989 100%);
    background: linear-gradient(to bottom, #a9a9a9 0%,#898989 100%);
    color: #808080 !important;
}

.disableCheck .label {
    color: #808080 !important;
}

.disableCheck input[type=checkbox]:checked + .radiocheck:after {
    content: '';
    color: #a9a9a9 !important;
}

