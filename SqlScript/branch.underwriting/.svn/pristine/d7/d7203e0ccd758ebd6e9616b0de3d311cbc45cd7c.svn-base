.loading2 {
    width: 100%;
    height: 100%;
    top: 0px;
    left: 0px;
    position: fixed; /*opacity: 0.7;     background-color: #fff;*/
    z-index: 150000;
    text-align: center;
    background-color: black;
    opacity: 0.2;
}

#loading2Image {
    background-image: url("Images/loading_b03.gif");
    background-position: top left;
    width: 111px;
    height: 111px;
    position: absolute;
    top: 50%;
    left: 47%;
}

.HeaderPopup {
    background: #11385b !important;
    background: url(data:image/svg+xml;base64,) !important;
    background: -moz-linear-gradient(top, #11385b 0%, #021122 100%) !important;
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#11385b), color-stop(100%,#021122)) !important;
    background: -webkit-linear-gradient(top, #11385b 0%,#021122 100%) !important;
    background: -o-linear-gradient(top, #11385b 0%,#021122 100%) !important;
    background: -ms-linear-gradient(top, #11385b 0%,#021122 100%) !important;
    background: linear-gradient(to bottom, #11385b 0%,#021122 100%) !important;
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#11385b', endColorstr='#021122',GradientType=0 ) !important;
}