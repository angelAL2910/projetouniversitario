/*.cont_risk_popup .radio input[type=radio]{display: none;}*/
.radio {
    font-family: Arial, Helvetica, sans-serif;
    font-size: 1em;
    color: #111;
    font-weight: bold;
}

    .radio label {
        display: block;
        cursor: pointer;
        position: relative;
        padding: 0;
        margin: 0;
        padding-left: 24px;
        font-size: 0.75em;
        height: 17px;
        line-height: 17px;
    }

    .radio input[type=radio] {
        display: none;
    }

    .radio label:before {
        content: "\2022";
        color: #aaa;
        display: block;
        width: 17px;
        height: 17px;
        text-align: center;
        position: absolute;
        left: 0px;
        bottom: 0px;
        background-color: #fff;
        border: 2px solid #aaa;
        border-radius: 25px;
        font-size: 3em;
        font-weight: 400;
        line-height: 0.40em;
    }

    .radio input[type=radio]:checked + label:before {
        content: "\2022";
        color: #007900;
        border: 2px solid #84ab0e;
    }

