/*contenido de los tabs*/
.content_tabs {
    background: #CBCDCF;
    position: relative;
    max-height: 345px;
    width: 100%;
    z-index: 10;
    overflow-x: hidden;
    overflow-y: auto;
    background-color: #CBCDCF;
    padding: 5px;
}
    /*afecta al contenido de los tabs*/
    .content_tabs input[type="text"], .content_tabs input[readonly="readonly"] {
        background: none;
    }

    .content_tabs table td select {
        background: none;
        color: #111;
    }

    .content_tabs table {
        background-color: #A6A9AE;
    }

        .content_tabs table td, .perfil table th { /*padding:1em 0.625em;*/
            border-bottom: 1px solid #CBCDCF;
            padding: 5px;
            font-size: 12px;
        }

        .content_tabs table th {
            color: #FFF;
            font-weight: bold;
            padding-left: 10px;
            padding-right: 0;
            text-align: left !important;
        }

    .content_tabs h2 {
        border-bottom: solid 1px #03294B;
        padding-bottom: 5px;
        padding-top: 8px;
        display: block;
        margin-bottom: 5px;
        color: #2E3338;
    }

    /*especial para quick mail*/
    .content_tabs .quick_mail input[type="text"], .quick_mail textarea, .quick_mail select {
        background: #FFFFFF !important;
        margin: 5px 0;
    }

    .content_tabs .quick_mail textarea {
        height: 113px;
    }

    .content_tabs .btn_grey {
        margin-left: 10px;
    }

    /*todos estan escondidos*/
    .content_tabs .profile, .content_tabs .list_mails, .content_tabs .list_phones, .content_tabs .list_address, .content_tabs .quick_mail {
        display: none;
    }

/*cuando este checked mostrar*/
.tabs input.tab-selector-1:checked ~ .content_tabs .profile,
.tabs input.tab-selector-2:checked ~ .content_tabs .list_mails,
.tabs input.tab-selector-3:checked ~ .content_tabs .list_phones,
.tabs input.tab-selector-4:checked ~ .content_tabs .list_address,
.tabs input.tab-selector-5:checked ~ .content_tabs .quick_mail {
    z-index: 100;
    display: block;
}

/*AFECTA A EDICION PERFIL*/
.edit_botones {
    display: none;
    overflow: hidden;
    background: #CBCDCF;
    width: 275px;
    overflow: hidden;
    margin-top: 5px;
}

    .edit_botones .btn_green {
        margin-right: 10px;
    }

.edit_mode, .changes_saved {
    padding-right: 28px;
    text-align: right;
    font-size: 13px;
    color: #D90000;
    font-weight: bold;
    display: block;
    max-width: 180px;
    overflow: hidden;
    position: absolute;
    top: 8px;
    right: 5px;
    z-index: 102;
    height: 20px;
    line-height: 26px;
    width: 140px;
    overflow: hidden;
    background-repeat: no-repeat;
    background-size: 20px 20px;
    background-position: right center;
}

.tabs .edit_mode {
    background-image: url(../images/edit_profile.svg);
}

.tabs .changes_saved {
    color: #158818;
    background-image: url(../images/changes_saved.svg);
}

.tabs .tabs input.tab-selector-5:checked ~ .content_tabs .edit_botones, .tabs input.tab-selector-5:checked ~ .content_tabs .edit_mode {
    display: none;
}

.tabs .content_tabs .edit_mode {
    display: none;
}

.tabs input.tab-selector-6:checked ~ .content_tabs .edit_mode {
    display: inline-block;
}

.tabs input.tab-selector-6:checked ~ .content_tabs input[type="text"],
.tabs input.tab-selector-6:checked ~ .content_tabs select,
.tabs input.tab-selector-6:checked ~ .content_tabs textarea {
    background: #FFF !important;
}

.tabs input.tab-selector-6:checked ~ .content_tabs .edit_botones {
    display: block;
}

.tabs input.tab-selector-5:checked ~ .content_tabs .edit_mode, .tabs input.tab-selector-5:checked ~ .content_tabs .edit_botones,
.tabs input.tab-selector-1:checked ~ .content_tabs .edit_mode, .tabs input.tab-selector-1:checked ~ .content_tabs .edit_botones {
    display: none;
}

.tabs input.tab-selector-1:checked ~ .content_tabs select, .tabs input.tab-selector-1:checked ~ .content_tabs input[type="text"] {
    background: none;
}

/*select para elegeri el tipo*/
.tabs input.tab-selector-6:checked ~ .content_tabs table td select.tipo {
    width: 65%;
    display: inline-block;
}

.add_option, .delete_option {
    display: none;
    width: 10%;
    height: 20px;
    cursor: pointer;
    text-indent: -999px;
    background-image: url(../images/add_new_option.svg);
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 18px 18px;
    border-radius: 6px;
    margin-left: 8px;
}

.delete_option {
    background: url(../images/delete_option.svg) no-repeat center center;
    background-size: 18px 18px;
}

.tabs input.tab-selector-6:checked ~ .content_tabs .add_option, .tabs input.tab-selector-6:checked ~ .content_tabs .delete_option {
    display: inline-block;
}

/*QUICK MAIL AND MAIL PART*/

/*barar que dice que tiene nuevos correos*/
a.new_emails {
    height: 38px;
    background: #03294B;
    color: #FFF;
    border-bottom: #000;
    border-bottom: solid 1px #B0B0B0;
    display: block;
    padding: 0 30px 0 10px;
    margin-top: 5px;
    font-size: 12px;
    font-weight: bold;
    line-height: 37px;
    overflow: hidden;
    position: relative;
}
/*animacion del circulo rojo que parpadea*/
.notify {
    display: block;
    position: absolute;
    top: 8px;
    right: 1.3333em;
    width: 1.85em;
    line-height: 1.875em;
    border-radius: 50%;
    font-size: 11px;
    font-weight: bold;
    text-align: center;
    background: #c20a0a;
    box-shadow: 0 0 0 0.25em #f53d3d;
    animation: blink 1s step-start 0s infinite;
    -webkit-animation: blink 1s step-start 0s infinite;
}

@keyframes blink {
    0% {
        box-shadow: 0 0 0 0.2em #f53d3d;
    }

    25% {
        box-shadow: 0 0 0 0.05em #f53d3d;
    }

    50% {
        box-shadow: 0 0 0 0.375em #f53d3d;
    }

    75% {
        box-shadow: 0 0 0 0.2em #f53d3d;
    }

    100% {
        box-shadow: 0 0 0 0.3em #f53d3d;
    }
}

@-webkit-keyframes blink {
    0% {
        box-shadow: 0 0 0 0.2em #f53d3d;
    }

    25% {
        box-shadow: 0 0 0 0.05em #f53d3d;
    }

    50% {
        box-shadow: 0 0 0 0.375em #f53d3d;
    }

    75% {
        box-shadow: 0 0 0 0.2em #f53d3d;
    }

    100% {
        box-shadow: 0 0 0 0.3em #f53d3d;
    }
}

