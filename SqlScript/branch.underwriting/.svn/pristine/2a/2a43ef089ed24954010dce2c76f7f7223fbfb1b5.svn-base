.panel_alertas {
    line-height: 38px;
    color: #093B72;
    min-height: 38px;
    font-size: 13px;
    font-weight: bold;
    text-shadow: 1px 1px 0px #fff;
    padding: 0 10px;
    text-align: right;
    /*border-bottom:solid 1px #888;*/
    border: solid 1px #c6c6c6;
    margin-bottom: 2px;
    background: #212730; /* Old browsers */
    background: -moz-linear-gradient(top, #ffffff 0%, #eaeaea 53%, #e1e1e1 53%, #e5e5e5 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(53%,#eaeaea), color-stop(53%,#e1e1e1), color-stop(100%,#e5e5e5)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, #ffffff 0%,#eaeaea 53%,#e1e1e1 53%,#e5e5e5 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, #ffffff 0%,#eaeaea 53%,#e1e1e1 53%,#e5e5e5 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, #ffffff 0%,#eaeaea 53%,#e1e1e1 53%,#e5e5e5 100%); /* IE10+ */
    background: linear-gradient(to bottom, #ffffff 0%,#eaeaea 53%,#e1e1e1 53%,#e5e5e5 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#e5e5e5',GradientType=0 ); /* IE6-9 */
}

    .panel_alertas span.titulo {
        display: inline-block;
        padding-right: 10px;
        color: #093B72;
    }

    .panel_alertas a.alertas {
        color: #093B72;
        display: inline-block;
        padding: 0 0 0 2px;
        margin: 0 5px;
        font-size: 12px;
        font-weight: bold;
        line-height: 37px;
        position: relative;
    }

        .panel_alertas a.alertas:hover {
            text-decoration: underline;
        }
    /*animacion del circulo rojo que parpadea*/
    .panel_alertas .alert, .panel_alertas .alert_green, .panel_alertas .alert_blue {
        display: inline-block;
        position: relative;
        margin-left: 8px;
        padding: 0 4px;
        min-width: 20px;
        height: 20px;
        line-height: 20px;
        border-radius: 30px;
        font-size: 11px;
        font-weight: bold;
        text-align: center;
        background: #c20a0a;
        -webkit-box-shadow: 0 0 0 3px #f53d3d;
        -moz-box-shadow: 0 0 0 3px #f53d3d;
        box-shadow: 0 0 0 3px #f53d3d;
        animation: blink 1s step-start 0s infinite;
        -webkit-animation: blink2 1s step-start 0s infinite;
        color: #FFF;
        text-shadow: none;
        text-decoration: none;
    }

    .panel_alertas .alert_green {
        animation: none;
        -webkit-animation: none;
        background: #4C7925;
        -webkit-box-shadow: 0 0 0 3px #80CF30;
        -moz-box-shadow: 0 0 0 3px #80CF30;
        box-shadow: 0 0 0 3px #80CF30;
    }

    .panel_alertas .alert_blue {
        background: #003F7D;
        animation: none;
        -webkit-animation: none;
        -webkit-box-shadow: 0 0 0 3px #3399FF;
        -moz-box-shadow: 0 0 0 3px #3399FF;
        box-shadow: 0 0 0 3px #3399FF;
    }

@keyframes blink2 {
    0% {
        box-shadow: 0 0 0 0.2em #f53d3d;
    }

    25% {
        box-shadow: 0 0 0 0.05em #f53d3d;
    }

    50% {
        box-shadow: 0 0 0 0.375em #f53d3d;
    }

    75% {
        box-shadow: 0 0 0 0.2em #f53d3d;
    }

    100% {
        box-shadow: 0 0 0 0.3em #f53d3d;
    }
}

@-webkit-keyframes blink2 {
    0% {
        box-shadow: 0 0 0 0.2em #f53d3d;
    }

    25% {
        box-shadow: 0 0 0 0.05em #f53d3d;
    }

    50% {
        box-shadow: 0 0 0 0.375em #f53d3d;
    }

    75% {
        box-shadow: 0 0 0 0.2em #f53d3d;
    }

    100% {
        box-shadow: 0 0 0 0.3em #f53d3d;
    }
}




