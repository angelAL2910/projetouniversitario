/*Paginación de los grids custom*/
.pagination {
    background: #F1F2F6 url(../images/pagination_background.png) repeat-x left top;
    border: solid 1px #c6c6c6;
    overflow: hidden;
    border-top: 0;
    padding: 0 10px;
    min-width: 380px;
    margin-right: -1px;
}

    .pagination p {
        float: left;
        font-weight: normal;
        text-align: left;
        font: 11px Verdana, Geneva, sans-serif;
        color: #201f35;
        padding: 5px 10px 0 0;
        margin: 3px 0;
    }

    .pagination .current {
        padding: 2px 5px 3px 5px;
        color: #201f35;
        display: block;
        text-decoration: none;
        cursor: default;
        margin: 5px 12px 0px 0px;
        float: left;
        font: 11px Verdana, Geneva, sans-serif;
        font-weight: bold;
        background: #f0f0f0;
        background: -moz-linear-gradient(top, #ffffff 0%, #e5e5e5 100%);
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(100%,#e5e5e5));
        background: -webkit-linear-gradient(top, #ffffff 0%,#e5e5e5 100%);
        background: -o-linear-gradient(top, #ffffff 0%,#e5e5e5 100%);
        background: -ms-linear-gradient(top, #ffffff 0%,#e5e5e5 100%);
        background: linear-gradient(to bottom, #ffffff 0%,#e5e5e5 100%);
        filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#e5e5e5',GradientType=0 );
        border: 1px solid #b0b0b0;
    }

    .pagination a.numbers {
        color: #003FFF;
        padding: 3px 5px;
        display: block;
        text-decoration: none;
        cursor: pointer;
        margin: 5px 12px 0px 0px;
        float: left;
        font: 11px Verdana, Geneva, sans-serif;
        font-weight: normal;
        text-align: center;
    }

        .pagination a.numbers:hover {
            text-decoration: underline;
        }

    .pagination .next, .pagination .prev, .pagination .next_dis, .pagination .prev_dis,
    .pagination .fwrd, .pagination .rewd, .pagination .fwrd_dis, .pagination .rewd_dis {
        display: block;
        float: left;
        width: 19px;
        height: 19px;
        text-indent: -999px;
        cursor: pointer;
        border: 0;
        margin: 5px 12px 5px 0px;
        background-image: url(../images/DXR.png);
        background-repeat: no-repeat;
    }

    .pagination .next {
        background-position: -39px -314px;
    }

    .pagination .prev {
        background-position: -100px -263px;
    }

    .pagination .next_dis {
        background-position: -60px -314px;
    }

    .pagination .prev_dis {
        background-position: -40px -263px;
    }

    .pagination .fwrd {
        background-position: -80px -263px;
    }

    .pagination .rewd {
        background-position: -20px -263px;
    }

    .pagination .fwrd_dis {
        background-position: -60px -263px;
    }

    .pagination .rewd_dis {
        background-position: -20px -314px;
    }


