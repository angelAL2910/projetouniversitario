/*BIG DAY*/
.calendar_agent .today {
    background-color: #CBCDCF;
    height: 75px;
    padding: 0.625em 80px 0.625em 10px;
    position: relative;
    color: #FFF;
    font-size: 2em;
    text-transform: capitalize;
}

    .calendar_agent .today span {
        font-size: 2.5em;
        position: absolute;
        right: 20px;
        top: -1px;
        text-align: right;
        display: block;
        width: 100px;
        height: 80px;
    }

.calendar_agent .cal {
    margin: 0 auto;
    width: 100%;
    color: #FFF;
    font-size: 11px;
}

    .calendar_agent .cal a {
        text-decoration: none;
        display: block;
    }

    .calendar_agent .cal caption {
        line-height: 32px;
        font-weight: bold;
        font-size: 13px;
        color: #FFF;
        text-align: center;
        background: #73B9FF;
        border-top: 1px solid #a0a0a0;
        border-bottom: 1px solid #FFF;
        -webkit-box-shadow: inset 0 1px rgba(255, 255, 255, 0.04);
        box-shadow: inset 0 1px rgba(104, 147, 36, 0.04);
    }

        .calendar_agent .cal caption a {
            display: block;
            line-height: 32px;
            padding: 0 10px;
            font-size: 15px;
            color: #e2e2e2;
        }

            .calendar_agent .cal caption a:hover {
                color: white;
            }

        .calendar_agent .cal caption .prev {
            float: left;
            width: 22px;
            height: 22px;
            background-image: url(../images/cal_arrows_prev.svg);
            background-repeat: no-repeat;
            background-position: center center;
            background-size: 16px 16px;
            overflow: hidden;
            margin-top: 5px;
            margin-left: 10px;
            cursor: pointer;
            display: block;
        }

        .calendar_agent .cal caption .next {
            float: right;
            width: 22px;
            height: 22px;
            background-image: url(../images/cal_arrows_next.svg);
            background-repeat: no-repeat;
            background-position: center center;
            background-size: 16px 16px;
            overflow: hidden;
            margin-top: 5px;
            margin-right: 10px;
            cursor: pointer;
            display: block;
        }

    .calendar_agent .cal th, .calendar_agent .cal td {
        text-align: center;
        vertical-align: middle;
    }

    .calendar_agent .cal th {
        font-size: 10px;
        color: #FFF;
        text-transform: uppercase;
        background: #73B9FF;
        font-family: Arial, Helvetica, sans-serif;
        height: 26px;
        border-bottom: 4px solid #212730;
    }

    .calendar_agent .cal td {
        font-size: 12px;
        background: #212730;
        color: #FFF;
    }

        .calendar_agent .cal td a {
            display: block;
            color: #FFF;
