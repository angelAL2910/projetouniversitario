.boton_next, .boton_prev {
    display: block;
    position: relative;
    text-transform: none;
    float: right;
    -webkit-border-radius: 20px;
    -moz-border-radius: 20px;
    border-radius: 20px;
    text-align: right !important;
    line-height: 28px;
    border-width: 3px;
    border-style: solid;
