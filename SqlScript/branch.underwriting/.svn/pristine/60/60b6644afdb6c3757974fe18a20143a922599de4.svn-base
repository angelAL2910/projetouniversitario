    background: -ms-linear-gradient(top, #00b2fc 0%,#02577b 100%);
    background: linear-gradient(to bottom, #00b2fc 0%,#02577b 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00b2fc', endColorstr='#02577b',GradientType=0 );
}

.boton_prev {
