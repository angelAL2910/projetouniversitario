/*.cont_risk_popup .radio input[type=radio]{display: none;}*/
.radio label {
	display: inline-block;
	cursor: pointer;
	position: relative;
	padding-left: 28px;
}

.radio input[type=radio] {
	display: none;
}

.radio label:before {
	content: "\2022";
	color: #aaa;
	display: inline-block;
	width: 20px;
	height: 20px;
	text-align: center;
	line-height: 17px;
	font-size: 42px;
	margin-right: 10px;
	position: absolute;
	left: 0;
	bottom: -3px;
	background-color: #fff;
	border: 2px solid #aaa;
}

.radio label:before {
	border-radius: 25px;
}


.radio input[type=radio]:checked + label:before {
	content: "\2022";
	color: #007900;
	font-size: 42px;
	text-align: center;
	border: 2px solid #007900 !important;
	line-height: 17px;
}

.radioSelect:before {
	content: "\2022" !important;
	color: #007900 !important;
	font-size: 42px !important;
	text-align: center !important;
	border: 2px solid #007900 !important;
	line-height: 17px !important;
	display: inline-block !important;
	width: 20px;
	height: 20px;
	margin-right: 10px;
	position: absolute;
	left: 0;
	bottom: -3px;
	background-color: #fff;
}



