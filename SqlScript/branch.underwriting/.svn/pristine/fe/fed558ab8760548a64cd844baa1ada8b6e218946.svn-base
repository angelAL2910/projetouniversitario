/*--------------NUEVOS BOTONES--------------*/

.boton_next, .boton_prev, .boton_next_dis, .boton_prev_dis {
    display: block;
    position: relative;
    text-transform: none;
    float: right;
    -webkit-border-radius: 20px;
    -moz-border-radius: 20px;
    border-radius: 20px;
    text-align: right !important;
    line-height: 22px;
    border-width: 2px;
    border-style: solid;
    border-color: #02577B;
    height: 28px;
    margin-top: 4px;
    color: #FFF;
    font-weight: bold;
    font-family: Arial, Helvetica, sans-serif;
    padding-left: 20px;
    padding-right: 35px;
    cursor: pointer;
    background: #00b2fc;
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top, #00b2fc 0%, #02577b 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#00b2fc), color-stop(100%,#02577b));
    background: -webkit-linear-gradient(top, #00b2fc 0%,#02577b 100%);
    background: -o-linear-gradient(top, #00b2fc 0%,#02577b 100%);
    background: -ms-linear-gradient(top, #00b2fc 0%,#02577b 100%);
    background: linear-gradient(to bottom, #00b2fc 0%,#02577b 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00b2fc', endColorstr='#02577b',GradientType=0 );
}

.boton_next_dis, .boton_prev_dis {
    border-color: #b0b0b0;
    color: #777;
    background: #fdfdfd;
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top, #fdfdfd 0%, #e0e0e0 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#fdfdfd), color-stop(100%,#e0e0e0));
    background: -webkit-linear-gradient(top, #fdfdfd 0%,#e0e0e0 100%);
    background: -o-linear-gradient(top, #fdfdfd 0%,#e0e0e0 100%);
    background: -ms-linear-gradient(top, #fdfdfd 0%,#e0e0e0 100%);
    background: linear-gradient(to bottom, #fdfdfd 0%,#e0e0e0 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fdfdfd', endColorstr='#e0e0e0',GradientType=0 );
}

.boton_prev, .boton_prev_dis {
    float: left;
    padding-right: 20px;
    padding-left: 35px;
}

