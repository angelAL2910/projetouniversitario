    z-index: 10;
    top: 100%;
    margin-top: 1px;
    left: -1px;
    min-width: 175px;
    border-left: 1px solid #EDEDED;
    border-right: 1px solid #EDEDED;
    border-bottom: 1px solid #EDEDED;
    border-radius: 3px;
    background: #fff;
}

    .menu-item ul:before {
        display: block;
