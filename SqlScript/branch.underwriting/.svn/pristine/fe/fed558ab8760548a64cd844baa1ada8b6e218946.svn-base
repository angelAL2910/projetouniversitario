.radiocheck_Cont input[type=checkbox] {
    display: none;
}

.radiocheck {
    padding-left: 30px;
    position: relative;
    cursor: pointer;
}

    .radiocheck:before {
        -webkit-appearance: none;
        padding: 9px;
        border-radius: 25px;
        display: inline-block;
        position: relative;
        cursor: pointer;
        content: "";
        position: absolute;
        left: 0px;
        top: -6px;
        width: 25px;
        height: 25px;
        background: #00b2fc;
        background: url(data:image/svg+xml;base64,PD94bWwgdmVyc2lvbj0iMS4wIiA/Pgo8c3ZnIHhtbG5zPSJodâ€¦EiIGhlaWdodD0iMSIgZmlsbD0idXJsKCNncmFkLXVjZ2ctZ2VuZXJhdGVkKSIgLz4KPC9zdmc+);
        background: -moz-linear-gradient(top, #00b2fc 0%, #02577b 100%);
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#00b2fc), color-stop(100%,#02577b));
        background: -webkit-linear-gradient(top, #00b2fc 0%,#02577b 100%);
        background: -o-linear-gradient(top, #00b2fc 0%,#02577b 100%);
        background: -ms-linear-gradient(top, #00b2fc 0%,#02577b 100%);
        background: linear-gradient(to bottom, #00b2fc 0%,#02577b 100%);
    }

    .radiocheck:after {
        content: '';
        border-radius: 20px;
        width: 15px;
        height: 15px;
        position: absolute;
        top: 5px;
        left: 5px;
        background-color: #fff;
        position: absolute;
        left: 5px;
        top: -1px;
        cursor: pointer;
    }

.radiocheck_Cont input[type=checkbox]:checked + .radiocheck:after {
    content: '\2022';
    font-size: 32px;
    color: #63bd0f;
    line-height: 17px;
