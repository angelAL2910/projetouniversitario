using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace WSVirtualOffice.Models.codes
{
    public class WSRidertype
    {
        public string ridertypename { get; set; }
        public string ridertypecode { get; set; }

        public  static List<WSRidertype> getWSriderTypesList()
        {
            List<WSRidertype> ridertypeslist=new List<WSRidertype>();
            ridertypeslist.Add(new WSRidertype(){ridertypecode="ADB",ridertypename="RIDERADB"});
            ridertypeslist.Add(new WSRidertype(){ridertypecode="TERM",ridertypename="RIDERTERM"});
            ridertypeslist.Add(new WSRidertype(){ridertypecode="FAMILIAR",ridertypename="RIDERFAMILIAR"});
            ridertypeslist.Add(new WSRidertype(){ridertypecode="REPATRIACION",ridertypename="RIDERREPATRIACION"});
            ridertypeslist.Add(new WSRidertype(){ridertypecode="SEPELTURALOTE",ridertypename="RIDERSEPELTURALOTE"});
            return ridertypeslist;
        }
    }
}