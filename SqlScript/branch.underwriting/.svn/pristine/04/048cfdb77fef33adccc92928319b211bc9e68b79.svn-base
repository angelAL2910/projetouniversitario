@media (min-width: 768px) and (max-width: 999px) {
}
@media (min-width: 270px) and (max-width: 999px) {
	.botones_inferiores 		{ width: 92%;}
	.botones_inferiores li 		{ margin-right: 1.2%;}
	.cabecera .titulos strong 	{ font-size: 0.9em;}
	.footer li					{ width: 30%;padding-right: 0px;margin-right: 1%;}
	.contenedor 				{ width: 96%;margin: 6% auto 0;}
}

@media (min-width: 270px) and (max-width: 720px) {
	.footer li			{ width: 100%;}
	.idiomas 			{ margin: 20px 20px 20px 0px;}
	.cabecera .titulos 	{ right: 20px;}
	.cabecera 			{ height: 184px;}
	.cabecera .titulos 	{ right: 20px; width: 45%;}
	.contenedor 		{ margin: 0px !important;}
	
}
@media (min-width: 550px) and (max-width: 800px) {
	
	.uno		{ left:-60px;}
	.tres		{ right:-60px;}
	
}

@media (min-width: 270px) and (max-width: 550px) {
	.contenedor 			{ width: 100%;}
	.botones_inferiores li 	{ width: 100%;}
	.cabecera .titulos 		{ top: 234px;text-align: center; right: 0px; width: 100%;}
	.cabecera .titulos strong {font-size: 1em;}
	.idiomas 				{ margin: 184px 6px 0px;width: 96%;}
	.cabecera 				{ height: 338px;background: #0A2739 url(../images/backg-logo-At320.png) no-repeat center top;}
	.boton 					{ width: 74%;}
	
	.pasos_container		{ height: 55px;}
	.uno					{ left:-35px;}
	.tres					{ right:-35px;}
}
