
/*-- MENU SUPERIOR ----------*/
.main_menu{
	width: 100%;
	float: left;
	/*min-height: 90px;*/
	position:relative; /*height:90px;*/ top:0;
	}
.main_menu > div{ float:left;}
.main_menu > div:first-child { width:290px; position:absolute; left:0; top:0;}
.main_menu > div:last-child { width:100%; padding-left:400px;}

.main_menu a.active, 
.nav li a.active{color:#FFF !important}

.main_menu{ height:auto;}

.main_menu {min-height: 90px;width: 100%;float: left;min-width: 410px;}
.main_menu {
	width: 100%; 
	float: left;
	background-color: #001B2B;
	background-image: -moz-linear-gradient(top,#001B2B,#001320);  
	background-image: -webkit-linear-gradient(top,#001B2B,#001320);
	background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0%,#001B2B), color-stop(100%,#001320));
	background-image: -o-linear-gradient(top,#001B2B,#001320);
	background-image: linear-gradient(to bottom,#001B2B,#001320);	
	}

.box_logo .logo{
	display:block;
	height:90px;
	background-image: url(../images/logo.png);
	background-repeat: no-repeat;
	background-position: left center;	
	background-size: 68%;
	width: 400px;
	background-position-x: 16px;
	}

.box_logo .logoSTL{
    display: block;
    height: 116px;
    background-image: url(../images/logo-STL.png);
    background-repeat: no-repeat;
    background-position: left center;
    background-size: 82%;
    width: 400px;
    background-position-x: 16px;
	}

.nav ul {
	width:100%;
	margin: 0;
	padding: 0;
	list-style: none;
	font-weight: 300;
	}
.nav li {
	float: left;
	width:16.5%;
	position:relative;
	text-align: center;
	}
/*.nav li:nth-child(11) {width: 10%;}*/

.nav li span{ display:block; z-index:5;}
.nav li span.icon {
	width:50px;
	height:50px;
	text-align:center;
	margin:0 auto;
	vertical-align:middle;
	background-repeat: no-repeat;
	background-position: center center;
	background-size: 90%;
	z-index:999;
	}
	
/*MENU HOVER  */		
a.active, .nav li a.active, .nav li a:hover {
	background: #2a9ec1; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top, #2a9ec1 0%, #082956 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#2a9ec1), color-stop(100%,#082956)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #2a9ec1 0%,#082956 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #2a9ec1 0%,#082956 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #2a9ec1 0%,#082956 100%); /* IE10+ */
	background: linear-gradient(to bottom, #2a9ec1 0%,#082956 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#2a9ec1', endColorstr='#082956',GradientType=0 ); /* IE6-8 */
	/*-webkit-box-shadow: inset 0px 1px 0px 0px rgba(255,255,255, .1), 0px 1px 0px 0px rgba(0,0,0, .1);
	-moz-box-shadow: inset 0px 1px 0px 0px rgba(255,255,255, .1), 0px 1px 0px 0px rgba(0,0,0, .1);
	box-shadow: inset 0px 1px 0px 0px rgba(255,255,255, .1), 0px 1px 0px 0px rgba(0,0,0, .1);
	border-left:1px solid #0C131F;*/   
	text-decoration:none; 
	}
/*MENU HOVER /> */	
	
	
/* NUEVO MENU HOVER */
	
.main_menu.menu2 a.active, .main_menu.menu2 .nav li a.active, .main_menu.menu2 .nav li a:hover {
  background-image: url(../images/top_menu/bg-active.png);
  background-repeat: no-repeat;
  background-size: 100px 100px;
  background-position: center center;
 /* border-left: 1px solid transparent;*/
  border-top: none!important;
  border-bottom: none!important;
  width: 100%;
  height: 98px;
}


/* Styling the toggle menu link and hiding it */
.nav .navtoogle{
	display: none;    
	width: 100%;
	text-align: left;
	color:#FFF;
	font-size: 1em;
	background: none;          
	border: none;

	border-top: 1px solid #020e23;
	border-bottom: 4px solid #020e23;

	padding-top:2px;
	padding-left: 30px;
	
	cursor: pointer;
	line-height:32px;              
	background:#01254c;
	-webkit-box-shadow: inset 0px 1px 0px 0px rgba(255,255,255, .1), 0px 1px 0px 0px rgba(0,0,0, .1);
	-moz-box-shadow: inset 0px 1px 0px 0px rgba(255,255,255, .1), 0px 1px 0px 0px rgba(0,0,0, .1);
	box-shadow: inset 0px 1px 0px 0px rgba(255,255,255, .1), 0px 1px 0px 0px rgba(0,0,0, .1);
	}
	

