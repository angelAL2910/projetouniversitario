using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace WSVirtualOffice.Models.querydata
{
    class Contacts
    {
        private String namec;
        private String idnoc;
        private String phnoc;
        private String emailc;
        private DateTime lastupdatedc;
        private String customernoc;

        public Contacts(String name, String idno, String phno, String email, DateTime lastupdated,String customerno)
        {
            this.namec = name;
            this.idnoc = idno;
            this.phnoc = phno;
            this.emailc = email;
            this.lastupdatedc = lastupdated;
            this.customernoc = customerno;
        }

        public String Name
        {
            get { return this.namec; }
            set { this.namec = value; }
        }

        public String Idno
        {
            get { return this.idnoc; }
            set { this.idnoc = value; }
        }

        public String Phno
        {
            get { return this.phnoc; }
            set { this.phnoc = value; }
        }

        public String Email
        {
            get { return this.emailc; }
            set { this.emailc = value; }
        }

        public DateTime Lastupdated
        {
            get { return this.lastupdatedc; }
            set { this.lastupdatedc = value; }
        }
        public String Customerno
        {
            get { return this.customernoc; }
            set { this.customernoc = value; }
        }
    }
}
