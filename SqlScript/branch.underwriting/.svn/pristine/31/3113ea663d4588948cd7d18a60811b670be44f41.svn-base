/*---walky---*/

*,
*::after,
*::before {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    padding: 0;
    margin: 0;
}

    *:focus {
        outline: 0px none transparent;
    }

.scrolldiv {
    overflow-y: hidden;
    height: 55px;
}

.dxpgMainTable {
    font-family: Arial, Helvetica, sans-serif;
    font-size: 11px;
    color: #111;
}

.dxpgControl, .dxgvControl {
    border: 0; /*border:1px solid #c6c6c6;*/
    width: 100%;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 11px;
    color: #111;
}

    .dxpgControl td input[type="checkbox"] {
        margin-right: 5px;
        vertical-align: middle;
    }

.PivotGrid {
    border: 0;
    width: 100%;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 11px;
    color: #000;
}

/*barra inferior que sale cuando arroja el resultado de una busqueda*/
/*.dxgvFilterBar, .dxgvControl {
    background: none;
}*/


/*filtros de agents y clients - inputs*/
input[type="text"].dxeEditArea {
    font-family: Arial, Helvetica, sans-serif;
    font-size: 12px;
    color: #111;
}

/*fondos de la barra de los filtros  - barra azul*/
.dxpgFilterArea {
    border-bottom: 1px solid #c0c0c0 !important;
    color: #111;
    border: 0;
    border-top: 0;
    background: #e4f5fc;
    background: -moz-linear-gradient(top, #e4f5fc 0%, #9fd8ef 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#e4f5fc), color-stop(100%,#9fd8ef));
    background: -webkit-linear-gradient(top, #e4f5fc 0%,#9fd8ef 100%);
    background: -o-linear-gradient(top, #e4f5fc 0%,#9fd8ef 100%);
    background: -ms-linear-gradient(top, #e4f5fc 0%,#9fd8ef 100%);
    background: linear-gradient(to bottom, #e4f5fc 0%,#9fd8ef 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e4f5fc', endColorstr='#9fd8ef',GradientType=0 );
}
/*fondos de los filtros que se arrastran de la barra azul*/
.dxpgHeader {
    border: 0px !important;
    color: Black;
    background-color: transparent !important;
    cursor: pointer;
    white-space: nowrap;
    text-align: left;
    color: #111;
    background: none;
}
/*esta es la barra gris debajo del azul - yearh - month*/
td.dxpgArea.dxpgDataArea, td.dxpgArea.dxpgColumnArea, .dxpLite .dxp-current {
    background: #f0f0f0;
    background: -moz-linear-gradient(top, #ffffff 0%, #e5e5e5 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(100%,#e5e5e5));
    background: -webkit-linear-gradient(top, #ffffff 0%,#e5e5e5 100%);
    background: -o-linear-gradient(top, #ffffff 0%,#e5e5e5 100%);
    background: -ms-linear-gradient(top, #ffffff 0%,#e5e5e5 100%);
    background: linear-gradient(to bottom, #ffffff 0%,#e5e5e5 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#e5e5e5',GradientType=0 );
    border-bottom: 1px solid gray;
}

.dxpgArea {
    background-color: #FFF !important;
}

td.dxpgArea.dxpgDataArea, td.dxpgArea.dxpgColumnArea, .dxpgRowFieldValue, .dxpgArea, .dxpgRowFieldValue {
    width: 200px;
    max-width: 200px !important;
}

    td.dxpgArea.dxpgColumnArea table, td.dxpgArea.dxpgColumnArea table td {
        background: none;
    }

/*esta es la cajita donde bajan los filtros, sombrita para la caja de los filtros activos o seleccionados de la franja azul, cuando bajan*/
td.dxpgArea.dxpgRowArea {
    -moz-box-shadow: 0px 2px 3px rgba(0, 0, 0, 0.1) inset;
    -webkit-box-shadow: 0px 2px 3px rgba(0, 0, 0, 0.1) inset;
    -o-box-shadow: 0px 2px 3px rgba(0, 0, 0, 0.1) inset;
    box-shadow: 0px 2px 3px rgba(0, 0, 0, 0.1) inset;
    border-bottom: 1px solid #c6c6c6;
    vertical-align: bottom;
}
    /*los filtros activos o seleccionados de la franja azul, cuando bajan*/
    td.dxpgArea.dxpgRowArea table td {
        color: #207E22;
        -moz-box-shadow: none;
        -webkit-box-shadow: none;
        -o-box-shadow: none;
        box-shadow: none;
        background: none !important;
        border: 0;
    }

/*cabecera principal degradado gris*/
.dxpgColumnFieldValue, .dxgvHeader, tr.dxgvFilterRow td.dxgv {
    width: 40px;
    min-width: 40px;
    color: #222;
    text-transform: capitalize;
    height: 28px;
    font-size: 11px;
    font-weight: bold;
    text-align: center;
    font-family: Arial, Helvetica, sans-serif;
    white-space: normal !important;
    vertical-align: middle;
    text-align:  center !important;
    background: #ededed;
    background: -moz-linear-gradient(top, #ededed 0%, #c6c6c6 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ededed), color-stop(100%,#c6c6c6));
    background: -webkit-linear-gradient(top, #ededed 0%,#c6c6c6 100%);
    background: -o-linear-gradient(top, #ededed 0%,#c6c6c6 100%);
    background: -ms-linear-gradient(top, #ededed 0%,#c6c6c6 100%);
    background: linear-gradient(to bottom, #ededed 0%,#c6c6c6 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ededed', endColorstr='#c6c6c6',GradientType=0 );
    text-shadow: 1px 1px 0px #f4f4f4;
    -moz-box-shadow: inset 0px 1px 0px 0px #f4f4f4;
    -webkit-box-shadow: inset 0px 1px 0px 0px #f4f4f4;
    box-shadow: inset 0px 1px 0px 0px #f4f4f4;
}

    .dxgvHeader td {
        white-space: normal !important;
        font-size: 1em;
    }

    .dxgvHeader span.dx-vam {
        display: none !important;
        height: 0 !important;
        width: 0 !important;
    }

.dxpgColumnGrandTotalFieldValue {
    border: solid 1px #AAA;
}

.dxpgRowTotalFieldValue {
    background-color: #FFF;
    font-weight: bold;
}

/*celdas de los grid - data*/
.dxpgCell, tr.dxgvDataRow td.dxgv, td.dxgv {
    font-size: 12px;
    border-left: 1px solid #c6c6c6 !important;
    border-bottom: 1px solid #c6c6c6 !important;
    border-top: 0 !important;
    border-right: 0;
    height: 28px;
    font-family: Arial, Helvetica, sans-serif;
    color: #000;
}

    .dxpgCell a, tr.dxgvDataRow td.dxgv a {
        color: #003FFF !important;
        text-decoration: none;
    }


tr:nth-child(even) td.dxpgCell, tr:nth-child(even) td.dxpgRowFieldValue {
    background-color: #E1F0D5;
}

tr:nth-child(odd) td.dxpgCell, tr:nth-child(odd) td.dxpgRowFieldValue {
    background-color: #FFF;
}

tr.dxgvDataRow:nth-child(even) td.dxgv {
