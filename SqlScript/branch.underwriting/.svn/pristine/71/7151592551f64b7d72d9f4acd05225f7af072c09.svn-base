      <xs:element type="xs:string" name="Tipo" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="Otros" minOccurs ="0" maxOccurs ="1"/>
    </xs:sequence>
  </xs:complexType>

