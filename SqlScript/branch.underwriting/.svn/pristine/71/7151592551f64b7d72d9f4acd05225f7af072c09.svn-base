      <xs:element type="xs:string" name="Tipo" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="TipoEdificacionOtra" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="TipoEdificioOtro" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="DescripcionRiesgo" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="Direccion" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:integer" name="EdadConstrucccion" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="MovimientoComercial" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="OrganizacionContable" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:integer" name="NoEmpleados" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="Horario" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="FormaDeOcupacion" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="AseguradoraAnterior" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="TipoEdificio" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="TipoEdificacion" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:date" name="FechaDeConstruccion" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:integer" name="CantidadDeNiveles" minOccurs ="0" maxOccurs ="1"/>
