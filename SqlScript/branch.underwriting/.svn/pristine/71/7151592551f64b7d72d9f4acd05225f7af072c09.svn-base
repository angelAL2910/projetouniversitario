      <xs:element type="xs:string" name="SiniestralidadDesc" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="Street" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="Sector" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="Municipio" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="Departamento" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:decimal" name="Longitud" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:decimal" name="Latitud" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="Texto" minOccurs ="0" maxOccurs ="1"/>
      <!-- Descripcion de los procesos -->
      <xs:element type="xs:string" name="Categoria" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="OpinionRiesgo" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="RecomendacionesTecnicas" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="RecomendacionesHechas" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="Descripcion" name="Descripcion" minOccurs ="0" maxOccurs ="unbounded"/>
      <xs:element type="Inspeccion" name="Inspeccion" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="Nivel" name="Nivel" minOccurs ="0" maxOccurs ="unbounded"/>
      <xs:element type="Perdida" name="Perdida" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="Peligro" name="Peligro" minOccurs ="0" maxOccurs ="unbounded"/>
      <xs:element type="Proteccion" name="Proteccion" minOccurs ="0" maxOccurs ="unbounded"/>
      <xs:element type="Exposures" name="Exposures" minOccurs ="0" maxOccurs ="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Descripcion">
    <xs:sequence>
      <xs:element type="xs:string" name="Detalle" minOccurs ="0" maxOccurs ="1"/>
      <xs:element type="xs:string" name="Tipo" minOccurs ="0" maxOccurs ="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Inspeccion">
