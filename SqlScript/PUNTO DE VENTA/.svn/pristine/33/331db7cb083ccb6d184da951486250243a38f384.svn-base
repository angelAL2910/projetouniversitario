<!-- POP UP ERRORES -->
<div class="modal cpopup popupError" id="errorContainer">
    <p id="errorTitle">
        Se han producido los siguientes errores:
    </p>
    <div class="popup02">
        <p id="errorMessage" style="display: none;">
        </p>
        <div class="divTableScroll">
            <ul id="errorList"></ul>
        </div>
        <button class="pbtn pb_red" onclick="$('#errorContainer').hide();">Cerrar</button>
    </div>
</div><!--cpopup-->
<!-- POP UP PREGUNTA -->
<div class="modal popupQuestion" id="questionContainer">
    <p id="questionTitle">
        Debe corregir los siguientes errores para continuar:
    </p>
    <span id="questionMessage"></span>
    <br />
    <div>
        <button id="questionAccept" class="pbtn pb_green">Aceptar</button>
        <button id="questionCancel" class="pbtn pb_red">Cancelar</button> @*onclick="$('#questionContainer').hide();"*@
    </div>
</div><!--cpopup-->
<!--POP UP SUCCESS-->
<div class="modal cpopup popupSuccess" id="successContainer">
    <p id="successTitle">
    </p>
    <div class="popup02">
        <p id="successMessage">
        </p>
        <div class=" tbl grid_popup">
            <div class="divTableScroll">
                <table id="successList"></table>
            </div>
        </div>
        <div class="mT20">
            <button class="pbtn pb_green" id="successContainerOkButton" style="width:48px; margin:0 auto;" onclick="$('#successContainer').hide();">Ok</button>
        </div>
        <div class="agradecimiento" id="successGratitud">
            <p>
                Gracias por depositar su confianza en
            </p>
            <div class="alogo2"></div>
        </div>
    </div>
</div><!--cpopup-->
<!-- POP UP WARNING -->
<div class="modal cpopup  popupWarning" id="warningContainer">
    <p id="warningTitle">
        Se han producido los siguientes errores:
    </p>
    <div class="popup02">
        <p id="warningMessage" style="display: none;">
        </p>
        <div class="divTableScroll">
            <ul id="warningList"></ul>
        </div>
        <button class="pbtn pb_yellow" onclick="$('#warningContainer').hide();">Cerrar</button>
    </div>
</div><!--cpopup-->