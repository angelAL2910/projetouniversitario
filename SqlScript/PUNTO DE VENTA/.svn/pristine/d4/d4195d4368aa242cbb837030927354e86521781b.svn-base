        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAgentService" />
                <binding name="BasicHttpBinding_IQueues" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://devagentservice.statetrustlife.com/AgentService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAgentService"
