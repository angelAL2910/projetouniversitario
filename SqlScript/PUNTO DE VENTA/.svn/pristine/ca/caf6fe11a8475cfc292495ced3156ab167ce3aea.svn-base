.LoadingEs {
    background-image: url(../images/loading.gif);
    background-position: top left;
    width: 13%;
    height: 24%;
    position: absolute;
    top: 50%;
    left: 42%;
}

.loading:before {
    content: '';
    background-color: #fff;
    position: absolute;
    left: 0;
    top: 0;
    opacity: 0.5;
    width: 100%;
    height: 100%;
    z-index: 8000;
    -webkit-border-radius: 15px;
    border-radius: 15px
}

.loading:after {
    content: '';
    position: fixed;
    background: url(../images/loading.gif) no-repeat center center;
    width: 80px;
    height: 80px;
    background-size: 100%;
    left: 48%;
    top: 48%;
    z-index: 9000;
}

