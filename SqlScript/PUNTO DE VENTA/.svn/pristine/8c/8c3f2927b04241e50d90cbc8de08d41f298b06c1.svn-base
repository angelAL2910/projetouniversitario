 <!-- Image Header -->
<div class="head-image mdl-card mt-3">
    <!-- mdl-shadow--2dp -->
    <div class="mdl-card__title mdl-card--expand">
        <h1 class="bnn_ttl">@ViewBag.TitlePage</h1>
    </div>
    <div class="mdl-card__actions p-0">
        <span class="head-image__filename">
            <button id="call_btn" class="mdl-button mdl-color-text--white mdl-button--raised mdl-js-button bg-success mr-1 mt-2 ml-2" type="button">
                <i class="fa fa-volume-control-phone fa-2x fa-fw" aria-hidden="true"></i>
                <strong class="x-hide">Llamame</strong>
            </button>
            <button id="chat_btn" type="button" class="mdl-button mdl-color-text--white mdl-button--raised mdl-js-button bg-info mr-1 mt-2">
                <i class="fa fa-commenting fa-2x fa-fw" aria-hidden="true"></i>
                <strong class="x-hide">Chat en linea</strong>
            </button>
        </span>
        <button data-toggle="modal" data-target="#nProcuctos" type="button" class="mdl-button mdl-button--raised mdl-js-button mb-1 mdl-js-ripple-effect mdl-color-text--white bg-dark float-right mt-2 mr-2">
            <i class="material-icons">shop</i>
            <strong class="x-hide">Nuestros</strong> Productos
        </button>
    </div>
    <div class="mdl-card__actions p-0 mt-3">
        @if (ViewBag.isAgentUser == false)
        {
            <div class="mdl-cell float-left">
                <button class="mdl-button w-100 mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent mdl-color-text--white text-capitalize p-0 mb-md-0 ">
                    <i class="material-icons">arrow_forward</i>Continuar con un Agente
                </button>
            </div>
        }
        <div class="badge badge-dark cNumber p-2 mr-2 float-right col-md-4"><strong class="x-hide">Cotización Auto </strong> No. <span id="spQuotationNumber"></span></div>
    </div>
</div>