<!--============| EJECUCION DEE JAVASCRIPT PARA MODAL DE FILTROS DE CLIENTES |============-->
<script>
    $(document).ready(function () {

        //Edad maxima para poder asegurar un Vehículo (16 anos)
        var currDate = moment(new Date());
        var minDate = currDate.add(16 * -1, 'years');
        //

        $('.dateOfBirth.datepicker').datepicker({
            changeMonth: true,
            changeYear: true,
            yearRange: "-80:+0",
            maxDate: new Date(minDate),
            onSelect: function (dateText) {
                var $parent = $(this).parent();
                $parent.addClass("is-dirty");
                $parent.removeClass('requerido');
                $parent.removeClass('erarequerido');
                $parent.addClass('erarequerido');
            }
        });

        $(document).on('focusout', '#IdentificationNumberFilter, #QuotationNumberFilter, #DateOfBirthFilter', function () {
            if ($(this).val() != '')
                $(this).addClass('Is-dirty');
        });

        $(document).on('click', '#closeCustomerFilter', function () {
            $('#filtroHistorico').val('1'); // lo envio a nueva cotizacion nuevamente
            $("#filtroHistorico").trigger("chosen:updated");
        });

        $('#ppCustomerFilter').on('hidden.bs.modal', function () {
            $('#filtroHistorico').val('2'); // lo envio a nueva cotizacion nuevamente
            $("#filtroHistorico").trigger("chosen:updated");
        })

        $(document).on('show.bs.modal', '#ppCustomerFilter', function () {
            $('#filtroHistorico').val('2');
            $("#filtroHistorico").trigger("chosen:updated");
        })

        $(document).on('click', '#ContinueToHistory', function () { return continueToHistory(); });

    });

</script>