<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="Dantelin Castro">
    <!-- <link rel="icon" href="images/favicon.png"> -->
    <title>Cotizador</title>
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/css/dtl.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/css/font-awesome.css">
    <link rel="stylesheet" href="~/Content/css/material.min.css">
    <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.blue-light_blue.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="~/Content/css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="~/Content/css/chosen.css">
    <link rel="stylesheet" href="~/Content/css/block-styles.css">
    <link href="~/Content/css/Master.css" rel="stylesheet" />
    <link href="~/Content/css/SecurityCss.css" rel="stylesheet" />
    <link href="~/Content/css/accordion.css" rel="stylesheet" />

    <!--jquery UI Css-->
    <link href="~/Content/css/jqueryui/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/css/jqueryui/jquery-ui.structure.min.css" rel="stylesheet" />
    <link href="~/Content/css/jqueryui/jquery-ui.theme.min.css" rel="stylesheet" />
    <!--jquery UI Css-->

    <script type="text/javascript" src="~/js/jquery-3.2.1.min.js"></script>

</head>
<body class="bg_atl_body">
    <div id="st-container" class="st-container" style="overflow-y: auto;">
        @if (ViewBag.UserOrigin == STL.POS.Frontend.Web.NewVersion.CustomCode.CommonEnums.UserOrigins.VO)
            {
                Html.RenderPartial("_SlideMenuAgent");
            }
        <div class="st-pusher">
            <div class="st-content">
                @if (ViewBag.UserOrigin == STL.POS.Frontend.Web.NewVersion.CustomCode.CommonEnums.UserOrigins.VO)
                {
                    <div id="st-trigger-effects">
                        @*onclick="GetUserProfileDataMVC(true); $('#menu-2').show();"*@
                        <button data-effect="st-effect-2" id="btnLoadProfile" class="show_profile_es"> </button>
                    </div>
                }
                <div id="scroll_2">
                    <div class="mdl-layout__container">
                        @{Html.RenderPartial("_HeaderNavBar");}
                        <main role="main" class="container main_cotiza_atl">
                            @{Html.RenderPartial("_HeaderOptions");}

                            <!--Contenedor-->
                            @RenderBody()
                            <!--Contenedor-->
                        </main>
                        @{Html.RenderPartial("_Footer");}

                        <!--============| POPUPS |============-->
                        @{Html.RenderPartial("_AllPopUps");}
                        <!--============| END POPUPS |============-->
                    </div>

                    <!-- TOOLTIPS -->
                    <div class="mdl-tooltip mdl-tooltip--top bg-primary" data-mdl-for="infoCB">
                        <strong class="">Ver Información de Coberturas</strong>
                    </div>
                    <!-- <div class="mdl-tooltip mdl-tooltip--top bg-primary" data-mdl-for="chat_btn">
                      <strong>Iniciar Chat</strong>
                    </div>
                    <div class="mdl-tooltip mdl-tooltip--top bg-primary" data-mdl-for="mail_btn">
                      <strong>Enviar un Correo</strong>
                    </div> -->
                </div>
            </div>
        </div>
    </div>

    <!--Hiddens Fields-->
    <input type="hidden" id="quotationID" value="" />
    <input type="hidden" id="QuotationNumber" value="" />
    <input type="hidden" id="QuotationCoreNumber" value="" />
    <input type="hidden" id="Financed" value="" />
    <input type="hidden" id="lastModelSelected" value="" />
    <input type="hidden" id="LastSelectedVehicleYearsOld" value="" />
    <input type="hidden" id="lastDeducibleSelected" value="" />
    <input type="hidden" id="oldAgentSelected" value="" />
    <input type="hidden" id="newAgentSelected" value="" />
    <!--//Hiddens Fields-->


    <script src="~/js/jquery-3.2.1.slim.min.js"></script>
    <script src="~/js/jquery-1.12.4.min.js"></script>
    <script src="~/js/vendor/popper.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/material.min.js"></script>
    <script src="~/js/chosen.jquery.min.js"></script>
    @*<script type="text/javascript" src="~/js/bootstrap-datepicker.js"></script>*@

    <script src="~/js/Security/jquery.nestedAccordion.js"></script>
    <script src="~/js/Security/jquery.nicescroll.min.js"></script>

    <script src="~/js/jquery-ui.js"></script>
    <script src="~/js/datepicker-es.js"></script>
    <script src="~/js/jquery.validate.js"></script>
    <script src="~/js/jquery.mask.js"></script>    
    <script src="~/js/MaskedInput.js"></script>
    <script src="~/js/CustomValidations.js"></script>
    
    <script src="~/js/moment.js"></script>
    <script src="~/js/moment-with-locales.js"></script>

    <script src="~/js/linq.min.js"></script>

    <script src="~/js/Custom.js"></script>
    <script src="~/js/jquery.qtip.min.js"></script>
    <script src="~/js/InfoAddDriver.js"></script>

    <script>
        $(document).ready(function () {
          
        });
    </script>
    <!-- // <script src="https://code.getmdl.io/1.3.0/material.min.js"></script> -->
</body>
</html>
