@{
    ViewBag.Title = "Atlántica Seguros";
}

<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="author" content="dcastro-amateo" />
    <title>@ViewBag.Title</title>

    <!--CSS Style-->
    @Styles.Render("~/Content/reset")
    @Styles.Render("~/Content/kickstart")
    @Styles.Render("~/Content/menuBundle")
    @Styles.Render("~/Content/amr")

    <!--SCRIPTS -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/underscore")
    @Scripts.Render("~/bundles/kickstart")
    @Scripts.Render("~/bundles/knockoutjs")
    @Scripts.Render("~/bundles/global")
    @Scripts.Render("~/bundles/jqueryvalidate")

    @*<script type="text/javascript" src="~/Scripts/knockout.simpleGrid.3.0.js"></script>
    <script type="text/javascript" src="~/Scripts/Areas/Auto/vehicleDuplicateViewModel.js"></script>*@
</head>
<body class="lngColor">
    <div class="wrapper">
        <div class="main_loging loging2">
            <div class="box_logo"><a href="#" class="logo"></a></div>

            <div id="menu" class="nav"></div>
            <!--menu class nav-->
        </div>
        <!--//main menu-->
        <br clear="all" />

        <div class="content_log tab_pane_container mT-100">
            @if (ViewBag.IsLogin == null) { 
            <div class="trans_label layoutLogin">
                <input type="text" class="" value="@ViewBag.CurrentUser" disabled="disabled">
                <a href="@Url.Action("LogoutPOS", "Account", new  { Area="Security" })" class="logout"></a>
            </div>
            }
            <div class="row blue">
                @RenderBody()
            </div><!--row blue agent_info_per-->

            <div class="master_container footer bR25 mT20">
                <div class="master_wrapper">
                    <div class="textwidget">
                        <div class="footer_col_products">
                            <div class="title">Productos</div>
                            <div class="body">
                                <ul style="width: 55%;">
                                    <li><strong>Para Usted</strong></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-usted/seguros-de-vida/">Seguros de Vida</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-usted/seguro-de-vehiculos/">Seguros de Vehículos</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-usted/vivienda/">Vivienda</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-usted/ultimos-gastos/">Últimos Gastos</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-usted/seguros-de-viajes/">Seguros de Viajes</a></li>
                                </ul>
                                <ul style="width: 45%;">
                                    <li><strong>Para su Empresa</strong></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-su-empresa/vida-colectiva/">Vida Colectiva</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-su-empresa/auto-comercial/">Auto Comercial</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-su-empresa/incendios-y-lineas-aliadas/">Incendios y Líneas Aliadas</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-su-empresa/lineas-comerciales/seguro-de-inundacion/">Seguro de Inundación</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-su-empresa/lineas-comerciales/extension-de-garantia/">Extensión de Garantía</a></li>
                                </ul>
                            </div>
                            <div class="icon"></div>
                        </div>

                        <div class="footer_col_products" id="salud">
                            <div class="title" id="salud">Salud Internacional</div>
                            <div class="body" id="salud">
                                <ul>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-usted/salud-internacional/">Servicio de Salud Internacional</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-usted/salud-internacional/aeroambulancia/">Aeroambulancia</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-usted/salud-internacional/ambulancia-local/">Ambulancia Local</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-usted/salud-internacional/vip-servicio-concierge/">VIP Servicio Conciérge</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/para-usted/salud-internacional/nuestros-servicios/">Nuestros Servicios</a></li>
                                </ul>
                            </div>
                            <div class="icon" id="salud"></div>
                        </div>

                        <div class="footer_col_products" id="asistencia">
                            <div class="title" id="asistencia">Asistencia Vial</div>
                            <div class="body" id="asistencia">
                                <ul>
                                    <li><a href="http://atlanticaseg.wgstudios.com/accidentes-y-asistencia/asistencia-vial/">Servicio de Grúas:<br />(809) 565-5591</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/asistencia-247/">Servicio al cliente 24/7</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/reclamaciones/para-usted/seguros-de-vehiculos/">Accidentes y Reporte de Reclamos</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/accidentes-y-asistencia/casa-del-conductor/">Casa del conductor</a></li>
                                </ul>
                            </div>
                            <div class="icon" id="asistencia"></div>
                        </div>

                        <div class="footer_col_products" id="filiales">
                            <div class="title" id="filiales">Empresas Relacionadas</div>
                            <div class="body" id="filiales">
                                <ul>
                                    <li><a href="http://atlanticaseg.wgstudios.com/filiales/grupo-statetrust/">Grupo StateTrust</a></li>
                                    <li><a href="http://www.statetrustlife.com/" target="_blank">www.statetrustlife.com</a></li>
                                    <li><a href="http://www.statetrustbank.eu/" target="_blank">www.statetrustbank.eu</a></li>
                                    <li><a href="http://www.statetrust.com/" target="_blank">www.statetrust.com</a></li>
                                </ul>
                            </div>
                            <div class="icon" id="filiales"></div>
                        </div>

                        <div class="footer_col_products" id="nosotros">
                            <div class="title" id="nosotros">Nosotros</div>
                            <div class="body" id="nosotros">
                                <ul>
                                    <li><a href="http://atlanticaseg.wgstudios.com/nosotros/que-es-un-seguro/">¿Qué es un seguro?</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/nosotros/importancia-de-seguros-para-una-familia/">Importancia del seguro de una familia</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/nosotros/ente-regulatorio/">Ente Regulatorio</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/nosotros/historia-de-atlantica-seguros/">Atlántica Seguros</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/nosotros/fortalezas-de-atlantica-seguros/">Fortalezas de Atlántica Seguros</a></li>
                                    <li><a href="http://atlanticaseg.wgstudios.com/nosotros/historia-del-grupo-statetrust/">Grupo StateTrust</a></li>
                                </ul>
                            </div>
                            <div class="icon" id="nosotros"></div>
                        </div>
                    </div><!--textwidget-->
                </div><!--master_wrapper-->
            </div>
        </div>

    </div>
    @{Html.RenderPartial("_Popups");}

</body>
</html>
