<!-- Popup Error -->
<div class="modal fade z9999" id="ppError" tabindex="-1" role="dialog" aria-labelledby="ppError" aria-hidden="true">
    <div class="modal-dialog " role="document">
        <div class="modal-content">
            <div class="block block-themed block-transparent mb-0">
                <div class="block-header bg-danger">
                    <h3 class="block-title d-block">
                        <i class="material-icons mr-2">&#xE033;</i><strong id="errorTitle">Se han producido los siguientes errores:</strong>
                    </h3>
                    <div class="block-options">
                        <button type="button" class="btn btn-block-option bg-danger cursor border border-white" data-dismiss="modal" aria-label="Close">
                            <i class="fa fa-close"></i>
                        </button>
                    </div>
                </div>
                <div class="block-content">
                    <div class="col-md-8 m-auto text-center">
                        <ul id="errorListContainer"></ul>
                    </div>
                    <div class="col-12 modal-footer ">
                        <button type="button" data-dismiss="modal" class="btn btn-danger cursor">Cerrar</button>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>

