@model List<Modelo.EN_AGENT_GP_COMMISSION_entity>
<br />
<br />
<div class="form-group">
    Buscar Vendor
    <div class="col-md-4">
     <input type="text" class="form-control" />
    </div>
</div>
<br />
<br />
<div class="container">
    <table class="table table-bordered">
        <thead>
            <tr>
                
                <th>Codigo Agente</th>
                <th>Vendor</th>
                <th>Nombre Completo</th>
                <th>Opciónes</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var a in Model)
            {
                <tr>                     
                    <td>@a.Agent_Code</td>
                    <td>@a.Vendor_Id </td>
                    <td>@a.Full_Name</td>                    
                    <td>
                        <input type="button" value="Ver" class="btn btn-primary"    onclick="MostrarVendedor('@HttpUtility.UrlEncode(Url.Encode(Newtonsoft.Json.JsonConvert.SerializeObject(a)))')" title="Ver Vendor" /> 
                        <input type="button" value="Editar" class="btn btn-success" onclick="EditarVendedor('@HttpUtility.UrlEncode(Url.Encode(Newtonsoft.Json.JsonConvert.SerializeObject(a)))')"  title="Ver Vendor" /> 
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

<!-- Button trigger modal -->
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal">
    Launch demo modal
</button>

<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                ...
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save changes</button>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/ControllerJs/Controller.js"></script>





