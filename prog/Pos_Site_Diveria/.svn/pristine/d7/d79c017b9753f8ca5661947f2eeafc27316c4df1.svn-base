	.data_id .tab-img 	{ width: 100% !important;height: 225px !important; margin: 0px !important;}
	.data_id 	 	{ height: 426px !important;}
	.data_id .imgid 	{ width: 48% !important;}
	.data_id .icon_fot2 	{ padding-left: 40px !important;}
	.data_id .tab-img-btn .mL-12-p { margin-left: 27% !important;}
	.data_id .tab-zoom-cont { height: 223px !important;}
	.data_id .box-cont 	{width: 100% !important;}
}

/* The "tablet" and "mobile" version */


@media (min-width:47.6em) and (max-width: 65em) {
	/* aqui se divide en 2 filas*/
	.nav li {
		display: block;
		float: left;
		width: 25%;
		line-height:12px !important;
		}
	.nav a {
		padding: 0.625em 0;
		vertical-align:middle;
		    height: 38px;
		text-align:left;
		position: relative;
		}
	.nav li span, 
	.nav li span.icon {
		display: inline-block;
		}
		
	/*iconos cuando se collapsa el menu*/
	.nav li span.icon {
		width: 32px;
		height:32px;
		position:absolute !important;
		left:2px;	
		top: 4px;
		margin:auto;
		}
	.dropdown:hover span.icon {
		width: 32px;
		height:32px;
		position:absolute !important;
		left:2px;	
		top: 4px;
		margin:auto;
		z-index:999;
		}
	
	.dropdown:hover > .drop-nav,
	.flyout:hover > .flyout-nav {display:block; z-index:999; width:100%; }
	
	.drop-nav 			{ color:#fff; z-index:99; background:#001827; top:90px; min-width:150px; width:100%; left:0px; -webkit-border-radius:10px; border-radius:10px; font-size:0.75em;}
	.drop-nav li		{ position:relative; float:left; height:35px; width:100%; white-space:nowrap;}
	
	.icon + span {
		font-size: 0.7em;
		margin-left:40px;
		position:static;
		text-align:left;
		margin-right:5px;
		margin-top:0px;
		}
		
	.nav li i {display: inline-block;}	
	

	
	
	.dropdown:hover li  		{  display: block; z-index:1; }
	.dropdown:hover li span  	{  display: block; z-index:1; }
	.dropdown:hover a .name_dd  {  display: block; z-index:999; }
	
	.nav li span.icon	{ z-index:0;}
	.dropdown > a		{ z-index: auto;}
}	


@media (max-width: 47.5em) {
	.nav li {
		float:none;
		width:100%;
		text-align:left;
		}
	.nav a {
		display: block;
		width: auto;
		}
	.nav i {
		position: relative;
		display: inline-block;
		margin: 0 auto;
		padding: 0.4em;
		}
	/*Nav*/
	.icon + span 				{font-size: 0.7em;margin-top: 0px; z-index:5;}
	.nav li span.icon 			{width: 60px;height: 60px; z-index:5;}
	.nav a 						{height: 72px;padding: 0.6em 0.25em;}
	
	
	/*MENU SE COLAPSA EN DROP DOWN*/
	.nav .navtoogle{
		margin: 0;
		display: block;
		}
	
	/* Cuando JavaScript esta deshabilitado, escondemos el menu */
	.no-js .nav ul {
		max-height: 35em;
		overflow: hidden;
		}
	.no-js .nav:active ul{
		max-height: 35em;
		overflow: visible;
		}
	
	/* Cuando JavaScript esta activado, escondemos el menu */
	.js .nav ul {
		max-height: 0em;
		overflow: hidden;
		}
	.js .nav:active ul{
		max-height: 0em;
		overflow: visible;
		}
	
	/* Visualizacion del menu cuando el usuario ha hecho clic en el boton*/
	.js .nav .active + ul {		
		max-height: 35em;
		overflow: visible !important;
		-webkit-transition: max-height .4s;
		-moz-transition: max-height .4s;
		-o-transition: max-height .4s;
		-ms-transition: max-height .4s;
		transition: max-height .4s;
		}
	.nav li span {
		display: inline-block;
		height: 100%;		
		}
	.nav li a{ text-align:left;}
	.nav li a > span { margin-left:5px;}
	.nav li a > span.icon{ width:30px; height:30px; padding-top:8px; margin-left:5px; margin-top: 4px;}
	.nav li a > span.icon + span {
		font-size: 0.7m;
		}
	.nav li:hover a{ text-align:left;}
	.nav li:hover a > span { margin-left:5px;}
	.nav li:hover a > span.icon{ width:30px; height:30px; padding-top:8px; margin-left:5px; margin-top: 4px;}
	.nav li:hover a > span.icon + span {
		font-size: 0.7em;
		}
	
	.dropdown span.txt_lb {
	  	/* margin: 55px auto 0 auto; */
	  	display: block;
	  	text-align: left;
	  	width: 100%;
	  	margin-top: 8px !important;
	  	position: absolute;
		}
	
	.dropdown:hover span.txt_lb {
	  	/* margin: 55px auto 0 auto; */
	  	display: block;
	  	text-align: left;
	  	width: 100%;
	  	margin-top: 8px !important;
	  	position: absolute;
		}
	
	.dropdown:hover ul  		{  display: block; z-index:1; }
	.dropdown:hover li  		{  display: block; z-index:1; }
	.dropdown:hover li span  	{  display: block; z-index:1; }
	.dropdown:hover a .name_dd  {  display: block; z-index:999; }
	
	.nav li span.icon	{ z-index:0;}
	.dropdown > a		{ z-index: auto;}
	
	.dropdown:hover > .drop-nav,
	.flyout:hover > .flyout-nav {display:block; z-index:999; width:100%; overflow:visible!important; }
	
	.drop-nav 			{ 
		color:#fff; 
		z-index:99; 
		background:#001827; 
		top:90px; 
		min-width:150px; 
		width:100%; 
		left:0px; 
		-webkit-border-radius:10px; 
		border-radius:10px; 
		font-size:0.9em;
		overflow:visible!important;
		}
		
	.drop-nav li		{ 
		position:relative; 
		float:left; 
		height:35px; 
		width:100%; 
		white-space:nowrap;
		overflow:visible!important;
		}
	
	.drop-nav li a {
		color: #fff !important;
		display: block;
		padding: 10px 10px 10px 10px;
		text-decoration: none;
		text-align: center !important;
		height: 35px!important;
		line-height:35px!important;
		text-shadow: none;
		font-weight: normal;
		}
		
	/*.flyout:hover a,*/
	.flyout-nav 		{  background: #fff;text-align: center;-webkit-border-radius: 10px;border-radius: 10px; overflow: hidden;}
	.drop-nav,
	.flyout-nav 		{ -webkit-box-shadow: 2px 2px 10px 0 #555;box-shadow: 2px 2px 10px 0 #555;}
		
	.flyout-nav li:nth-child(even) 	{ background-color: #E5E5E5!important;}
	.flyout-nav li:nth-child(odd) 	{ background-color: #fff!important;}
	.flyout-nav li  				{ float: left; min-width: 200px;}
	.flyout-nav li a 				{ text-align: center;color: #001827 !important;padding: 10px 30px;}
	.flyout-nav li a:hover 			{ color: #fff !important; background: #0099FF;}
	
		
	/*degradado menu colapsa*/	
	.nav li a {height:38px;padding:0;line-height:25px;}

	.main_menu{ height:auto;}
	
	/*tabs*/
	.contenedor_tabs ul.tabs_ttle					{ overflow:hidden}
	.contenedor_tabs ul.tabs_ttle li				{ display:block; float:left; width:50%; height:36px; overflow:hidden;}	
	.contenedor_tabs ul.tabs_ttle.dvddo_in_6 li 	{ width:33.3333333333%; height:36px; overflow:hidden;}
	.orange ul.tabs_ttle.dvddo_in_5 li 				{ width:25%; height:36px; overflow:hidden;}
	.contenedor_tabs ul.tabs_ttle.dvddo_in_11 li 	{ width:33.3333333333%; height:36px; overflow:hidden;}
	.contenedor_tabs ul.tabs_ttle.dvddo_in_4 li 	{ width:50%; height:36px; overflow:hidden;}
	.contenedor_tabs ul.tabs_ttle li a				{ font-size:11px; padding:1em 0;}	
	
	/*excepciones*/
	.contenedor_tabs ul.tabs_ttle li.dos_lineas a	{ width:120px; margin:0 auto; padding:0}
	.contenedor_tabs ul.tabs_ttle li.dos_lineas 	{ padding:5px 0}
	
	.data_id .tab-img 	{ width: 100% !important; margin: 0px !important;}
	.data_id 	 	{ height: 372px !important;}
	.data_id .imgid 	{ width: 68% !important;}
	.data_id .icon_fot2 	{ padding-left: 40px !important;}
	.data_id .tab-img-btn .mL-12-p { margin-left: 27% !important;}
	
	.main_menu > div:last-child {width: 100%;padding-left:0px;}
}

@media (max-width: 28.75em) {	
	.contenedor_tabs ul.tabs_ttle{overflow:hidden}
	.contenedor_tabs ul.tabs_ttle li { display:block; float:left; width:50%; height:36px; overflow:hidden;  padding-top:1em}
	.contenedor_tabs ul.tabs_ttle.dvddo_in_6 li, .contenedor_tabs ul.tabs_ttle.dvddo_in_4 li{width:50%; height:36px; overflow:hidden;}
	
	.contenedor_tabs ul.tabs_ttle li a{font-size:11px; line-height:12px; padding:0}
	
	.pagination p{ float:none; display:block}
}
