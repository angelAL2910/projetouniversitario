.btn_agentes {
  width: 222px;
  height: 66px;
  padding: 5px;
  border: 2px #0090ee solid;
  margin: 0px 15px 12px 0px;
  display: block;
  box-sizing: border-box;
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  border-radius: 6px;
  /*background-image: url(../images/btn_agentes_bg.jpg);
  background-repeat: repeat-x;
  background-position: top;*/
  
  background: #f7fcff; /* Old browsers */
  /* IE9 SVG, needs conditional override of 'filter' to 'none' */
  background: url(data:image/svg+xml;base64,);
  background: -moz-linear-gradient(top, #f7fcff 0%, #86d5fe 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f7fcff), color-stop(100%,#86d5fe)); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top, #f7fcff 0%,#86d5fe 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top, #f7fcff 0%,#86d5fe 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top, #f7fcff 0%,#86d5fe 100%); /* IE10+ */
  background: linear-gradient(to bottom, #f7fcff 0%,#86d5fe 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f7fcff', endColorstr='#86d5fe',GradientType=0 ); /* IE6-8 */
	}
.btn_agentes img {
  float: right;
