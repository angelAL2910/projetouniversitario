/*-- MENU SUPERIOR ----------*/
.main_lobby.lobby{
	width: 100%;
	float: left;
	/*min-height: 90px;*/
	position:relative; /*height:90px;*/ top:0;
	font-family:Arial, Helvetica, sans-serif;
	}
.main_lobby.lobby > div{ float:left;width:100%;}


.main_lobby.lobby a.active, 
.main_lobby.lobby .nav li a.active{color:#333 !important}



.main_lobby.lobby {
	width: 100%;
	min-width: 320px;
	height:128px;
	float: left;
	
		
	}

.main_lobby.lobby .box_logo .logo{
	display:block;
	height:100px;
	background-image: url(../images/logo-02.png);
	background-repeat: no-repeat;
	background-position: center center;	
	background-size: 90%;
	background-color: transparent;
	width:320px;
	margin-top:4px;
	}

.main_lobby.lobby .nav ul {
	width:100%;
	margin: 0;
	padding: 0;
	list-style: none;
	font-weight: 300;
	float:right;
	}
.main_lobby.lobby .nav li {
	float: left;
	width:10%;
	position:relative;
	text-align: center;
	}
/*.nav li:nth-child(11) {width: 10%;}*/

.main_lobby.lobby .nav li span{ display:block; z-index:99;}
.main_lobby.lobby .nav li span.icon {
	width:100px;
	height:100px;
	text-align:center;
	margin:0 auto;
	vertical-align:middle;
	background-repeat: no-repeat;
	background-position: center center;
	background-size: 90%;
	z-index:500;
	margin-top:4px;
	position:relative;
	}
	
	
.main_lobby.lobby a.active, .main_lobby.lobby .nav li a.active, .main_lobby.lobby .nav li a:hover {
	background-image:url(../images/bg-active.png);
	background-repeat:no-repeat;
	background-size:100px 100px;
	background-position:center center;
	border-left: none!important;
	border-top:none!important;
	border-bottom:none!important;
	width:100%;
	height:98px;
	z-index:99;
	position:relative;
	}
	



/* Styling the toggle menu link and hiding it */
.main_lobby.lobby .nav .navtoogle{
	display: none;    
	width: 100%;
	text-align: left;
	color:#FFF;
	font-size: 1em;
	background: none;          
	border: none;

	border-top:none!important;
	border-bottom:none !important;

	padding-top:2px;
	padding-left: 30px;
	
	cursor: pointer;
	line-height:32px;              
	background:#01254c;
	-webkit-box-shadow: inset 0px 1px 0px 0px rgba(255,255,255, .1), 0px 1px 0px 0px rgba(0,0,0, .1);
	-moz-box-shadow: inset 0px 1px 0px 0px rgba(255,255,255, .1), 0px 1px 0px 0px rgba(0,0,0, .1);
	box-shadow: inset 0px 1px 0px 0px rgba(255,255,255, .1), 0px 1px 0px 0px rgba(0,0,0, .1);
	}
	

