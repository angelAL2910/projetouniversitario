.lespeciales 	{ background-image: url(../images/top_menu/ico-lespeciales.png);}
.lcomerciales 	{ background-image: url(../images/top_menu/ico-lcomerciales.png);}
.servicios		{ background-image: url(../images/top_menu/ico-servicios.png);}
.tuagente 		{ background-image: url(../images/top_menu/tuagente.png);}
.cotizar 		{ background-image: url(../images/top_menu/ico-cotizar.png);}
.main_lobby.lobby .icon + span{ font-size:0.75em; text-align:center; margin-top:0px;font-weight: 600 !important; color:#333; }

.main_lobby.lobby li.dropdown:hover .icon + span{ font-size:0.75em; text-align:center; margin-top:0px;font-weight: 600 !important; color:#FFFFFF; }
.main_lobby.lobby .nav a {
	height:90px;
	display: block;    
	color: rgba(255, 255, 255, .9);
	text-decoration: none;
	padding: 0.3em 0.25em;
	/*text-shadow: 0px 1px 2px rgba(0,0,0, .65);*/
	text-align:center;
	/*border-left: 1px solid transparent; #001320*/
	}



/* SUB MENU MENU SUPERIOR-------------------- */
.name_dd {
	width: 100%;
	height:86px;
	color: #fff;
	font-weight: normal;
	top: 60px;
	left: 0;
  	line-height: 70px;
	text-shadow: 1px 1px 5px #001827;
	-webkit-border-radius: 10px 10px 0 0;
	border-radius: 10px 10px 0 0;
	
	background: #11385b;
	background: url(data:image/svg+xml;base64,PD94bWwgdmVyc2lvbj0iMS4wIiA/Pgo8c3ZnIHhtbG5zPSJodâ€¦EiIGhlaWdodD0iMSIgZmlsbD0idXJsKCNncmFkLXVjZ2ctZ2VuZXJhdGVkKSIgLz4KPC9zdmc+);
	background: -moz-linear-gradient(top, #11385b 0%, #021122 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#11385b), color-stop(100%,#021122));
	background: -webkit-linear-gradient(top, #11385b 0%,#021122 100%);
	background: -o-linear-gradient(top, #11385b 0%,#021122 100%);
	background: -ms-linear-gradient(top, #11385b 0%,#021122 100%);
	background: linear-gradient(to bottom, #11385b 0%,#021122 100%);
	-webkit-box-shadow: 0 0 2px 1px rgba(13, 46, 77, 0.55) inset;
	-moz-box-shadow: 0 0 2px 1px rgba(13, 46, 77, 0.55) inset;
	box-shadow: 0 0 2px 1px rgba(13, 46, 77, 0.55) inset;
	font-size: 0.9em;
	z-index:-1;
	}

.main_lobby.lobby .nav li.dropdown .drop-nav li.flyout a:hover {z-index: 0;  background-image: none !important; background-size: auto !important; width:100%; height:35px !important;}

.dropdown .drop-nav li,
.flyout 					{position: relative;}

.drop-nav,
.flyout-nav,
.name_dd 					{position: absolute;display: none;z-index:1;}
	
.drop-nav 					{ color:#333; z-index:99; background:#001827; top:125px; min-width:100px; width:100%; left:0px; -webkit-border-radius:10px; border-radius:10px; font-size:0.75em;}
.main_lobby.lobby .nav .drop-nav li				{ position:relative; float:none; height:35px; width:100%; white-space:normal;}


.drop-nav li a {
    color: #fff !important;
    display: block;
    padding: 10px 10px 10px 10px;
    text-decoration: none;
	text-align: center;
	height: 35px;
	text-shadow: none;
	font-weight: normal;
	}
.drop-nav li:last-child 		{ height: 40px;}
.drop-nav li:nth-child(even) 	{ background-color: #E5E5E5;}
.drop-nav li:nth-child(odd) 	{ background-color: #fff;}
.drop-nav li a 					{ text-align: center;color: #001827 !important;}
.drop-nav li a:hover 			{ color: #fff !important; background-color: #0099FF;}
	
.dropdown span.txt_lb 			{ margin:55px auto 0 auto;  display:block; text-align:center; width:100%; margin-top:0 !important; position:relative;}
.dropdown:hover span.txt_lb 	{ margin:55px auto 0 auto;  display:block; text-align:center; width:100%; margin-top:0px !important; position:relative;}

.dropdown:hover > .drop-nav,
.flyout:hover > .flyout-nav 	{ display:block; z-index:999; width:100%; }

.dropdown:hover li span  		{ display: block; z-index:999; }
.dropdown:hover .name_dd  		{ display: block; z-index:0; }

.flyout-nav {left:0; top:35px;}

/*.flyout:hover a,*/
.flyout-nav 		{  background: #fff;text-align: center;-webkit-border-radius: 10px;border-radius: 10px; overflow: hidden;}
.drop-nav,
.flyout-nav 		{ -webkit-box-shadow: 2px 2px 10px 0 #555;box-shadow: 2px 2px 10px 0 #555;}

.flyout-nav li:nth-child(even) 	{ background-color: #E5E5E5;}
.flyout-nav li:nth-child(odd) 	{ background-color: #fff;}
.flyout-nav li  				{ float: left; min-width: 200px;}
.flyout-nav li a 				{ text-align: center;color: #001827 !important;padding: 10px 30px;}
.flyout-nav li a:hover 			{ color: #fff !important; background: #0099FF;}



.navbar nav>ul>li.menu-item {
    padding: 9px 12px;
    cursor: pointer;
    border-left: 0!important;
    border-right: 0!important;
    color: rgba(0,0,0,.5);
}


