    text-align: center;
    height: 35px;
    text-shadow: none;
    font-weight: 600;
	font-size:12px !important;
	line-height:34px !important;
	}
	
	
	
	nav>ul>li.menu-item ul li .drop-nav li a:hover 			{color: #fff !important;background-color: #0099FF!important;}
	nav>ul>li.menu-item ul li .drop-nav li:nth-child(odd) 	{background-color: #fff!important;}
	nav>ul>li.menu-item ul li .drop-nav li:nth-child(even) 	{background-color: #E5E5E5!important;}

	.menu-item ul li .drop-nav li a:not([role=button]) 		{background:none!important;}
	
	
