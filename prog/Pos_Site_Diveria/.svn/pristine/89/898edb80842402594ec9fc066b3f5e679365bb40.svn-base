/*barar que dice que tiene nuevos correos*/
a.new_emails {
    height: 38px;
    background: #484D55;
    color: #FFF;
    border-bottom: #000;
    border-bottom: solid 1px #B0B0B0;
    display: block;
    padding: 0 30px 0 10px;
    margin-top: 5px;
    font-size: 12px;
    font-weight: bold;
    line-height: 37px;
    overflow: hidden;
    position: relative;
}
/*animacion del circulo rojo que parpadea*/
.notify {
    display: block;
    position: absolute;
    top: 8px;
    right: 1.3333em;
    width: 1.85em;
    line-height: 1.875em;
    border-radius: 50%;
    font-size: 11px;
    font-weight: bold;
    text-align: center;
    background: #c20a0a;
    box-shadow: 0 0 0 0.25em #f53d3d;
    z-index: 55;
    animation: blink 1s step-start 0s infinite;
    -webkit-animation: blink 1s step-start 0s infinite;
}

@keyframes blink {
    0% {
        box-shadow: 0 0 0 0.2em #f53d3d;
    }

    25% {
        box-shadow: 0 0 0 0.05em #f53d3d;
    }

    50% {
        box-shadow: 0 0 0 0.375em #f53d3d;
    }

    75% {
        box-shadow: 0 0 0 0.2em #f53d3d;
    }

    100% {
        box-shadow: 0 0 0 0.3em #f53d3d;
    }
}

@-webkit-keyframes blink {
    0% {
        box-shadow: 0 0 0 0.2em #f53d3d;
    }

    25% {
        box-shadow: 0 0 0 0.05em #f53d3d;
    }

    50% {
        box-shadow: 0 0 0 0.375em #f53d3d;
    }

    75% {
        box-shadow: 0 0 0 0.2em #f53d3d;
    }

    100% {
        box-shadow: 0 0 0 0.3em #f53d3d;
    }
}

