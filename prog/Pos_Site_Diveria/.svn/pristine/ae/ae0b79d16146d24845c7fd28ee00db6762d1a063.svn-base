<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="author" content="dcastro-amateo" />
    <title>@ViewBag.Title</title>

    <!--CSS Style-->
    @Styles.Render("~/Content/vo")
    @Styles.Render("~/Content/accordion")
    @Styles.Render("~/Content/kickstart")
    @Styles.Render("~/Content/reset")
    @Styles.Render("~/Content/dtl")
    @Styles.Render("~/Content/amr")
    @Styles.Render("~/Content/component")
    @Styles.Render("~/Content/jqueryuilib")
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />

    <!-- END STYLES -->
    <!--SCRIPTS -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/underscore")
    @Scripts.Render("~/bundles/kickstart")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/knockoutjs")
    @Scripts.Render("~/bundles/jqueryvalidate")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/global")
    @Scripts.Render("~/bundles/momentjs")
        

    @*<script type="text/javascript" src="~/Scripts/knockout.simpleGrid.3.0.js"></script>
        <script type="text/javascript" src="~/Scripts/Areas/Auto/vehicleDuplicateViewModel.js"></script>*@

    <script type="text/javascript">
    
    </script>

    <!-- END SCRIPTS -->
</head>

<body class="car">

    <!-- Contenedor General -->


    <div id="st-container" class="st-container" style="overflow-y: auto;">
        @if (ViewBag.UserOrigin == STL.POS.Data.UserOrigins.VO && ViewBag.UserType != STL.POS.Data.UserType.WebUser)
        {
            Html.RenderPartial("_SideMenuVO");
        }
        <div class="st-pusher">
            <div class="st-content">
                @{Html.RenderPartial("_MainMenu");}
                <!--boton abre y cierra el canvas-->
                @if (ViewBag.UserOrigin == STL.POS.Data.UserOrigins.VO && ViewBag.UserType != STL.POS.Data.UserType.WebUser)
                {
                    <div id="st-trigger-effects">
                        <button data-effect="st-effect-2" id="btnLoadProfile" class="show_profile_es" onclick="GetUserProfileDataMVC(true);"> </button>
                    </div>
                }
                @*class="st-content-inner"*@
                <div id="scroll_2">
                    <div class="main clearfix">
                        <div class="contenido_derecha">
                            <!--wrapper de las columnas-->
                            <div class="grid grid-pad">
                                <div id="divBody" class="">
                                    <div class=" wrapper-fluid box_shw pventa ">
                                        <!--wrapper-->
                                        <!--Div que se mostrara cuando se muestre el poppup de el marbete -->
                                        <div class="overlaypp" style="display:none;"></div>

                                        <!-- Contenedor -->
                                      
                                        @RenderBody()
                                        @{Html.RenderPartial("_Popups");}
                                        <!--// Contenedor -->
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="ajaxLoader">
        <div>
            <!--<div>Espere un momento...</div>-->
            <img src="~/Images/loading_Atlantica.gif" />
        </div>
    </div>
    <!--// Contenedor General -->



    <script type="text/javascript">
        $(document).ready(function () {

            $('#cbp-ntaccordion').cbpNTAccordion();

        });
    </script>
</body>
</html>