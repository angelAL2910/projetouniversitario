                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IQueues"
                contract="QueuesServices.IQueues" name="BasicHttpBinding_IQueues" />-->
            <endpoint address="https://atlqueueservices.atlantica.do/WCFServices/Queues_Services.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_IQueues" contract="QueuesServices.IQueues" name="BasicHttpsBinding_IQueues" />
        </client>
    </system.serviceModel>
