function productViewModel() {
    var self = this;

    //Third party
    self.name = ko.observable();
    self.othersProperty = ko.observable(0).money();
    self.onePersonDeath = ko.observable(0).money();
    self.anotherPersonDeath = ko.observable(0).money();
    self.passengerDeath = ko.observable(0).money();
    self.anotherPassengerDeath = ko.observable(0).money();
    self.finance = ko.observable(0).money();

    //Self damages
    self.driverRiskLimit = ko.observable(0).money();
    self.collisionRolloverLimit = ko.observable(0).money();
    self.fireTheftLimit = ko.observable(0).money();
    self.comprehensiveRiskLimit = ko.observable(0).money();
    self.glassBreakageLimit = ko.observable().money(0);
    self.specialEquipmentLimit = ko.observable(0).money();

    self.driverRiskDeducible = ko.observable();
    self.collisionRolloverDeducible = ko.observable();
    self.fireTheftDeducible = ko.observable();
    self.comprehensiveRiskDeducible = ko.observable();
    self.glassBreakageDeducible = ko.observable();
    self.specialEquipmentDeducible = ko.observable();

    self.driverRiskDeducibleList = ko.observableArray();
    self.collisionRolloverDeducibleList = ko.observableArray();
    self.fireTheftDeducibleList = ko.observableArray();
    self.comprehensiveRiskDeducibleList = ko.observableArray();
    self.glassBreakageDeducibleList = ko.observableArray();
    self.specialEquipmentDeducibleList = ko.observableArray();

}