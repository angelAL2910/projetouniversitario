@charset "utf-8";
/* CSS Document */

.bg {
    background-image: url(img_girl.jpg);
    background-image: url(../images/bg-point.png);
    background-repeat: repeat;
    height: 100%;
}


.bg.new-style.pv_dtl .st-pusher {
    background-color: #fff!important;
    background:#fff!important;

}

.bg.new-style.pv_dtl .st-pusher::before {
    float: left;
    width: 100%;
    background: url(img_girl.jpg);
    background: url(../images/bg-point.png);
    background-repeat: repeat;
    height: 100%;
}

.bg.new-style.pv_dtl .st-effect-2.st-menu-open .st-pusher::before {
    content: '';
    width: 100%;
    height: 100%;
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    background: rgba(0,0,0, .7)!important;
    z-index: 9999;
}




.new-style .head-image > .mdl-card__actions.blue {
    height: 52px;
    padding: 16px;
    background:#3896ca;
}

.new-style .mdl-button--raised {
    background: transparent;
    box-shadow: none;
}

.new-style .badge {
    color:#FFFFFF;
	font-weight: normal !important;
}

.new-style .badge span{
	font-weight: 700 !important;
}



.new-style .bg-primary, .bg-dark-az {
    /* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#004471+0,00578c+100 */
	background: #FFFFFF; /* Old browsers */
	background: -moz-linear-gradient(left, #FFFFFF 0%, #FFFFFF 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(left, #FFFFFF 0%,#FFFFFF 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to right, #FFFFFF 0%,#FFFFFF 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#FFFFFF', endColorstr='#FFFFFF',GradientType=1 ); /* IE6-9 */
	}
	
.new-style .medium-blue{color:#3896ca !important;}
.new-style .medium-blue-bg{background:#3896ca !important;}
.new-style .medium-blue-outline{border-color:#3896ca !important;}

.new-style .medium-blue-outline:hover {
    color:#FFFFFF !important;
    border-color:#3896ca !important;
	background:#3896ca !important;
}

.dark-blue-bg{background-color:#004471!important;}


.new-style .mdl-layout__tab-bar-button {
    background-color:transparent !important;
}

.new-style.pv_dtl .bg-white{background-color:#FFFFFF!important; background: #FFFFFF!important;}

.new-style.pv_dtl .pv_auto nav.navbar {
    border-bottom: #3896ca 3px solid;
}

.new-style .nav.pncp .select2-selection__rendered, 
/*.new-style .nav.pncp .select2-container--bootstrap .select2-selection--single .select2-selection__placeholder,*/ 
.new-style .nav.pncp .select2-selection--single {
    color: #3896ca !important;
}



.new-style .nav.pncp .select2-selection--single, 
.new-style .nav.pncp .chosen-container-single .chosen-single {
    color: #3896ca !important;
    border: 1px solid #3896ca !important;
}

.new-style .nav.pncp .select2-selection--single:hover, 
.new-style .nav.pncp .select2-container--bootstrap .select2-selection--single:hover .select2-selection__placeholder,
.new-style .nav .chosen-container-single .chosen-single:hover {
    color: #FFFFFF !important;
    background-color: #3896ca !important;
    border-color: #3896ca !important;
}

.new-style .nav.pncp .select2-container--bootstrap .select2-selection--single .select2-selection__placeholder {
    color: #3896ca !important;
}

.new-style .btn-outline-info {
    color: #3896ca;
    background-color: transparent;
    background-image: none;
    border-color: #3896ca;
}

.new-style .btn-outline-info:hover {
    color: #FFFFFF;
    background-color: #2475a2;
    background-image: none;
    border-color: #2475a2;
}

.new-style h1.bnn_ttl {
    font-weight: 600;
    color: #0b5888;
    text-align: center;
    display: block;
    margin: 0 auto;
    text-transform: uppercase;
    text-shadow: #0c3d5a 1px 1px 0px, #0d3348 0px 2px 0px, #082d42 0px 3px 0px, rgba(140, 140, 140, 0.498039) 0px 4px 0px, #787878 0px 0px 0px, rgba(0, 0, 0, 0.498039) 0px 5px 10px;
}

.new-style .logo {
    display: block;
    height: 85px;
    background-image: url(../images/logo-wb.png);
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100%;
    background-color: transparent;
    width: 260px;
    position: relative;
}

.new-style .mdl-radio__inner-circle, .new-style .mdl-switch.is-checked .mdl-switch__thumb, .new-style .mdl-checkbox.is-checked .mdl-checkbox__tick-outline, .new-style.pv_dtl .mdl-radio__inner-circle 
 .new-style.pv_dtl .block.block-themed>.block-header, .new-style.pv_dtl .mdl-textfield__label:after, .new-style.pv_dtl .accordStyles a[aria-expanded="true"] .mdl-chip {
    background-color: #00578c !important;
}

.new-style .pv_auto a[aria-expanded="true"] .rd_theme_dtl {
    border: 4px solid #00578c;
    color: #00578c !important;
}

.new-style .pv_auto .mdl-chip {
    background-color: #11385b !important;
    color: #FFFFFF !important;
}

.new-style .accordStyles .mdl-chip__text {
    color: #c5dde8;
    font-size: 18px;
}

.new-style .text-primary {
    color: #ffffff !important;
}

.new-style  .disabledAccordionTab {
    pointer-events: none;
    opacity: 0.9;
}

.new-style .bg-info {
    background-color: #3896ca!important;
}

.new-style .nav .select2-selection--single, .new-style .nav .chosen-container-single .chosen-single {
    color: #17a2b8 !important;
    background-color: transparent !important;
    background-image: none !important;
    font-weight: 400;
    white-space: nowrap;
    vertical-align: middle;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    border: 1px solid #3896ca !important;
    padding: .375rem .75rem;
    font-size: 1rem;
    line-height: 1.5;
    border-radius: .25rem;
    transition: background-color .15s ease-in-out,border-color .15s ease-in-out,box-shadow .15s ease-in-out;
    webkit-box-shadow: none !important;
    box-shadow: none !important;
}

.new-style.pv_dtl .pv_auto footer
/*.new-style .bg-dark-az*/  {
    border-top: #3896ca 5px solid;
    background: #004471!important; /* Old browsers */
	background: -moz-linear-gradient(left, #004471 0%, #00578c 100%)!important; /* FF3.6-15 */
	background: -webkit-linear-gradient(left, #004471 0%,#00578c 100%)!important; /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to right, #004471 0%,#00578c 100%)!important; /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#004471', endColorstr='#00578c',GradientType=1 )!important; /* IE6-9 */
}

.new-style .bg-primary,
.new-style .bg-dark-az{
    background: #004471!important; /* Old browsers */
	background: -moz-linear-gradient(left, #004471 0%, #00578c 100%)!important; /* FF3.6-15 */
	background: -webkit-linear-gradient(left, #004471 0%,#00578c 100%)!important; /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to right, #004471 0%,#00578c 100%)!important; /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#004471', endColorstr='#00578c',GradientType=1 )!important; /* IE6-9 */
}

.new-style a.bg-info:focus, a.bg-info:hover {
    background-color: #2475a2!important;
}


.mh-60 {min-height:60px;}


.mdl-mega-footer--link-list li, .mdl-mega-footer__link-list li:first-child {
    font-size: 20px;
}

.new-style .mdl-button {min-width: 140px;}

.new-style .swatch-success {
    color: #fff;
    background-color: #28a76d!important;
}

.new-style .table tr th:nth-child(1),
.new-style .table thead th:nth-child(1),
.new-style .table thead th:nth-child(2),
.new-style .table tbody tr:last-child th:nth-child(1),
.new-style .table tbody tr:last-child th:nth-child(2),
.new-style .table tbody tr:last-child td:nth-child(1),
.new-style .table tbody tr:last-child td:nth-child(2) { border-right:2px solid #e9ecef !important;}

.new-style .table tr {line-height:32px;}

.new-style .mdl-cell--12-col a.btn.btn-outline-info {min-width:230px;}

.new-style .totales, .new-style.pv_dtl tr.bg-success {
    background-color: #39dc92 !important;
    color: #0e583f!important;
}

.new-style .lite-blue {color: #FFFFFF!important; background-color:#3896ca !important;}
.new-style .bg-medium-blue {background: #116aa5 !important;}
.new-style .bg-medium-blue:hover {background: #24739e !important;}

.new-style .mdl-layout.is-upgraded .mdl-layout__tab.is-active {
    color: rgb(255,255,255);
    font-weight: 600;
    font-size: 16px;
    /* line-height: 54px; */
    padding-top: 5px;
}

.new-style .table {color:#5f5f5f !important;}