.select_pink, .input_pink{ border:2px solid #cc0099; -webkit-box-shadow: 0 2px 5px 0 #808080; box-shadow: 0 2px 5px 0 #808080; background-color:#FFFFFF; text-align:center; height:40px; border-radius:6px; width:100%}
.textarea_pink{ border:2px solid #cc0099; -webkit-box-shadow: 0 2px 5px 0 #808080; box-shadow: 0 2px 5px 0 #808080; background-color:#FFFFFF; text-align:center; border-radius:16px; width:100%; top:-8px; position:relative}
.section_pink{ border:2px solid #cc0099; -webkit-box-shadow: 0 2px 5px 0 #808080; box-shadow: 0 2px 5px 0 #808080; background-color:#FFFFFF; text-align:center; border-radius:8px; width:100%; top:-8px; margin:2px 0px; position:relative; height:36px}
.margin_form { margin : 5px 0px; }
.btn_pink { -webkit-box-shadow: 0 2px 5px 0 #808080; box-shadow: 0 2px 5px 0 #808080; background-color:#cc0099; text-align:center; border-radius:6px; width:20%; color:#FFFFFF; float:right; margin: 4px 8px; border:none; height:22px ; line-height:0;}
.btn_green { -webkit-box-shadow: 0 2px 5px 0 #808080; box-shadow: 0 2px 5px 0 #808080; background-color:#70AD47; text-align:center; border-radius:6px; width:40%; color:#FFFFFF; border:none;}
.text_left { text-align:left; }

/* segunda */ 
.iframe_pdf { display: block; color: #222; padding: 5px 12px; width: 100%; float: left; min-height: 300px; height: 100%; border: 1px solid #ea700d; border-radius: 20px;}
.iframe_pdf iframe { height:300px; width:450px; }
.pdf_content h1{ color : #FF0062; text-align:center; padding-top:30px; font-size:40px; font-weight:bold}
.bullets { padding-left:80px; }
.bullets li{ font-weight:bold; font-size:24px; zoom: 1; list-style: disc outside none;}
.icon_pdf_reclamos { float:right; padding-right:20px; }
.pull_left { float:left; }

/* Tercer */ 
.separator_1 { margin-top:60px;}
.btn_border_pink { border:1px solid #cc0099; height:30px; background-color:#FFFFFF; -webkit-box-shadow: 0 2px 5px 0 #808080; box-shadow: 0 2px 5px 0 #808080; padding:0px 20px; border-radius: 10px;}
.pull_right { float:right }
.input_md { width:60% !important; }
.reduce_label_plus_input { height:26px;}
.icon_home { background: url(../images/casa.png) no-repeat 99% center #FFFFFF !important;}
.icon_work { background: url(../images/maletin.png) no-repeat 99% center #FFFFFF !important;}
.icon_mobile { background: url(../images/mobile_phone.png) no-repeat 99% center #FFFFFF !important;}
.icon_checked { background: url(../images/cotejo.png) no-repeat 99% center #FFFFFF !important;}