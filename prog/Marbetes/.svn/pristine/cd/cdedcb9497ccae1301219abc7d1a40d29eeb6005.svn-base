}
	
.banner_warning{
	display: block;
  color: #222;
  padding: 50px 30px;
  width: 30% !important;
  text-align: center !important;
  float: left;
  min-height: 240px;
  height: 100%;
  border: 1px solid #ff3399;
  border-radius:20px;
  margin-top:15px;
  margin-left:50px;
  position:relative;
  
}

#warning{
position:absolute;	
top:0;
left:-50px;
width:90px;
background: #fff;
border-radius:100%;
}
 
.banner_warning p{
