  padding: 1px 2.5px;
  display: inline-block;
}

#Doc_Actions li a img {
  width: 30px;
  float:right;
}

.Adicionales{
  display: block;
  color: #222;
  padding: 5px 12px;	
  width:60% !important;
  text-align:center !important;
  float:left;
  height:100%;
}




iframe{
margin: 0;	
}
.Adicionales td a{
  text-decoration:underline !important;
  color: #ff3399 !important;
  cursor:initial !important;
}

.Adicionales table{
	width:100%;	
}
.data_G.Adicionales 		{ overflow-y: hidden; overflow-x: auto;}
.data_G.Adicionales table 	{  min-width: auto;}
.data_G.Adicionales tr th span						{ line-height: 37px; !important}

