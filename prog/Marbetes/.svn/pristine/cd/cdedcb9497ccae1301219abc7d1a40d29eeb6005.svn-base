.condiciones{
  display: block;
  color: #222;
  padding: 5px 12px;	
  
  text-align:center !important;
  float:left;
  min-height:300px;
  height:100%;
}

.condiciones:nth-child(1){
width:47% !important;	
}

.condiciones:nth-child(2){
width:53% !important;	
}


.condiciones iframe{
	display: block;
  border: 1px solid #ea700d;
  border-radius:20px;
  width: 100%;
  height: 100%;
  min-height: 275px;
}

iframe{
margin: 0;	
}
.condiciones td a{
  text-decoration:underline !important;
  color: #ff3399 !important;
  cursor:initial !important;
}

.condiciones table{
	width:100%;	
}
.data_G.condiciones 		{ overflow-y: hidden; overflow-x: auto;}
.data_G.condiciones table 	{  min-width: auto;}
.data_G.condiciones tr th:nth-child(1) span,
.data_G.condiciones tr th:nth-child(2) span			{ line-height: 17px; !important}
.data_G.condiciones tr th:nth-child(1) span 		{ min-width: 95px;}
.data_G.condiciones tr th span						{ line-height: 17px; !important}


#Doc_Actions{
  display: block;
  width: 30px;
  margin: 0 auto;
  padding: 20px 0;
}

