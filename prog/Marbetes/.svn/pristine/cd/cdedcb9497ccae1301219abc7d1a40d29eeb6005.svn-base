@charset "utf-8";
/* CSS Document */



/*//////////////Lightbox Modal ////////*/
#lightbox{
	width:100%;
	height:100%;
	background:rgba(0,0,0,0.5);
	position:absolute;
	z-index:200;
	top:0;
	right:0;
	bottom:0;
	left:0;
	display:none;
}


#lightbox .image_container{
	position:relative;
	width: 100%;
	height:100%;
	max-height:700px;
	max-width:800px;
	padding:15px;
	background:#fff;
	margin:0 auto;
	top:100px;
	right:0;
	bottom:0;
	left:0;
	
}

