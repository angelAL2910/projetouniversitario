/**
Dropdown de Bootstrap
**/
.dropdown.open .dropdown-menu li {
    width: 100%;
}
.dropdown.open .dropdown-menu li span {
    text-align: left;
    width: 100%;
}
.dropdown-menu > li > a:hover, .dropdown-menu > li > a:focus {
    background-color: #26abe3;
    color: white;
}
.dropdown-menu > li > a:hover span, .dropdown-menu > li > a:focus span {
    color: white;
}

@media screen and (min-width:768px) and (max-width:1520px) {
    .navbar-nav {
        width: 100%;
    }
    /* aqui se divide en 2 filas*/
    .nav li {
        display: block;
        float: left;
        width: 50%;
        line-height:12px !important;
    }
    .nav a {
        padding: 0.625em 0;
        vertical-align:middle;
        height: 38px;
        text-align:left;
        position: relative;
    }
    .nav li span,
    .nav li span.icon {
        display: inline-block;
    }
    /*iconos cuando se collapsa el menu*/
    .nav li span.icon {
        width: 38px;
        height:38px;
        position:absolute !important;
        left:2px;
        top: 4px;
        margin:auto;
    }
    .dropdown:hover span.icon {
        width: 32px;
        height:32px;
        position:absolute !important;
        left:2px;
        top: 4px;
        margin:auto;
        z-index:985;
    }
    .dropdown:hover > .drop-nav,
    .flyout:hover > .flyout-nav {display:block; z-index:985; width:100%; }

    .drop-nav { color:#fff; z-index:99; background:#001827; top:90px; min-width:150px; width:100%; left:0px; -webkit-border-radius:10px; border-radius:10px; font-size:0.75em;}
    .drop-nav li { position:relative; float:left; height:35px; width:100%; white-space:nowrap;}
    .icon + span {
        font-size: 0.9em;
        margin-left:40px;
        position:static;
        text-align:left;
        margin-right:5px;
        margin-top:0px;
    }
    .nav li i {display: inline-block;}
    .dropdown:hover li  		{  display: block; z-index:1; }
    .dropdown:hover li span  	{  display: block; z-index:1; }
    .dropdown:hover a .name_dd  {  display: block; z-index:985; }
    .nav li span.icon	{ z-index:0;}
    .dropdown > a		{ z-index: auto;}


    .main_menu.menu2 .nav li span.icon 	{ width: 38px; height: 38px; margin-top: 0px !important;}

    .main_menu.menu2 a.active,
    .main_menu.menu2 .nav li a.active,
    .main_menu.menu2 .nav li a:hover 	{ background-size: 38px 38px !important; background-position: 0px 0px !important;}
    .main_menu.menu2 .nav li 			{ width: 33%;}
    .nav li span.icon 					{ left: 4px;}
    .main_menu.menu2 .box_logo .logo 	{ margin-top: 0px !important;}

    .dropdown.open .dropdown-menu li {
        width: 100%;
    }
    .dropdown.open .dropdown-menu li span {
        margin-top: 5px;
        text-align: left;
        width: 100%;
    }
    .item-blanco {
        display: none !important;
     }
}
