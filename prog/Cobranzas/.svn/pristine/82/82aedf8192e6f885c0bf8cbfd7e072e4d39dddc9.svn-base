*/
.main_menu.menu2 .nav li span{
    display:block;
    z-index:5;
    /*float:left;*/
}
.main_menu.menu2 .nav li span.icon {
    width: 55px;
    height: 55px;
    text-align: center;
    margin: 0 auto;
    vertical-align: middle;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100%;
    z-index: 985;
    display: block;
    
    position: absolute;
    left: 5px;
    top: 5px;
}
