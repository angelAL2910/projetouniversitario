    text-align: left;
}
.main_menu.menu2 .nav ul.dropdown-menu li {
    height: auto;
}
.main_menu.menu2 .nav li:hover {
    background-color: #fff;
}
.main_menu.menu2 .nav li a {
}

/******************************************************************************************/
/********  AQUI DEFINIMOS EL ANCHO DE LOS ELEMENTOS LI PARA EL NUMERO DE OPCIONES  ********/
/******************************************************************************************/
.main_menu.menu2 .nav li {
    float: left;
    width:12.5%;
    position:relative;
    text-align: center;
}
/******************************************************************************************/
/******************************************************************************************/

.main_menu.menu2 .nav li:nth-child(11) {width: 10%;}
.main_menu.menu2 .nav a {
    display: block;
    width: auto;
}
.nav i {
    position: relative;
    display: inline-block;
    margin: 0 auto;
    padding: 0.4em;
}
.main_menu.menu2 .icon + span { padding-top:0.1em; line-height:1.1em}

.main_menu.menu2 .dropdown:hover > .drop-nav,
.main_menu.menu2 .flyout:hover > .flyout-nav {display:block; z-index:980; width:100%; }

.drop-nav {
    color:#fff;
    z-index:99;
    background:#001827;
    top:90px;
    min-width:150px;
    width:100%;
    left:0px;
    -webkit-border-radius:10px;
    border-radius:10px;
    font-size:0.75em;
}
.drop-nav li {
    position:relative;
    float:left;
    height:35px;
    width:100%;
    white-space:nowrap;
}
