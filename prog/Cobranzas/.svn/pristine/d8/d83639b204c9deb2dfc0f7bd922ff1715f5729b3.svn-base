class Utility {
    /**
     * // -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- // >>
     * vbarrera | 08 Mar 2019
     * Devuelve una cadena de texto con el codigo html de un obejto instanciado desde jquery
     */
    static GetHtml(jqueryInstance) {
        return $("<div></div>").append(jqueryInstance).html();
    };
};