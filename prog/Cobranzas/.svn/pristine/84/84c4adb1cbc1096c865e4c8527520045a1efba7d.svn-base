*.table-responsive .table.table-striped tfoot tr td{
	color:transparent !important;
	padding-left:1px!important;
}

.table-responsive .table.table-striped tfoot tr td a, 
.table-responsive .table.table-striped tfoot tr td span{
	position:relative;
	float:left;
	padding:6px 12px;
	margin-left:-1px;
	line-height:20px;
	text-decoration:none;
	color:#999;
	background-color#fff;
	border:1px solid #ddd;
	
}

.table-responsive .table.table-striped tfoot tr td a:focus,
.table-responsive .table.table-striped tfoot tr td a:hover,
.table-responsive .table.table-striped tfoot tr td span{
	z-index:3;
	color:#fff;
	cursor:default;
	background-color:#337ab7;
	border-color:#337ab7;
}