  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>sysflex</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Poliza">
            <Value>=Parameters!Poliza.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- Declare @Poliza varchar(100)    = '1-05-400182'

Declare @FechaVenceFinal Datetime
SET NOCOUNT ON

SELECT
	a.Ramo,
	a.Poliza,
	ISNULL(SUBR.FechaInicioVigencia, GETDATE()) AS InicioVigencia,
	ISNULL(SUBR.FechaFinVigencia, GETDATE()) AS FinVigencia,
	ValorFacturado = ISNULL(ctas.ValorFactura, 0),
	BalanceFacturado = ISNULL(ctas.BalanceFactura, 0),

	CONVERT(numeric(18, 6), 0) AS DiasPagado,
	CONVERT(numeric(18, 6), 0) AS ValorDias,
	CONVERT(numeric(18, 6), 0) AS DiasFaltante --,
INTO #1
FROM dbo.P_PolizaHeader AS a (NOLOCK)
LEFT JOIN (SELECT
	Sr.Ramo Ramo,
	MIN(dt.FechaInicioVigencia) FechaInicioVigencia,
	MAX(dt.FechaFinVigencia) FechaFinVigencia,
	dt.Compania,
	dt.Cotizacion
FROM P_PolizaDetail dt (NOLOCK)
INNER JOIN dbo.p_SubRamo Sr
	ON Sr.Ramo = dt.Ramo AND
	Sr.SubRamo = dt.SubRamo
GROUP BY	dt.Compania,
			dt.Cotizacion,
			Sr.Ramo) SUBR
	ON SUBR.Compania = a.Compania AND SUBR.Cotizacion = a.Cotizacion
	AND SUBR.ramo = a.ramo
LEFT JOIN (SELECT
	co.Compania,
	co.Poliza,
	ValorFactura = SUM(co.Valor),
	BalanceFactura = SUM(co.Valor) - ISNULL((SELECT
		SUM(P.VALOR)
	FROM dbo.IN_Documentos P
	WHERE P.POLIZA = co.Poliza AND P.TipoDoc = 9), 0)
FROM dbo.in_documentocobrar co (NOLOCK)
WHERE co.Compania = 30 AND co.Concepto &lt;&gt; 'CANCELADA POR FALTA PAGO'
GROUP BY	co.Compania,
			co.Poliza) ctas
	ON ctas.Compania = a.Compania
	AND ctas.Poliza = a.Poliza

WHERE a.Compania = 30 AND a.Poliza =
	CASE ISNULL(@Poliza, '')
		WHEN '' THEN a.Poliza ELSE @Poliza
	END

SELECT
	*,
	DATEDIFF(DAY, InicioVigencia, FinVigencia) AS DiasVigencia,
	(ISNULL(ValorFacturado, 0) - ISNULL(BalanceFacturado, 0)) AS ValorPagado,
	GETDATE() AS FechaVence INTO #2
FROM #1

UPDATE #2
SET ValorDias = ISNULL(ValorFacturado, 0) / CASE ISNULL(365, 0)
	WHEN 0 THEN 1 ELSE ISNULL(365, 0)
END

UPDATE #2
SET DiasPagado = ISNULL(ValorPagado, 0) / CASE ISNULL(ValorDias, 0)
	WHEN 0 THEN 1 ELSE ValorDias
END

UPDATE #2
SET DiasPagado = 0
WHERE diaspagado &lt; 0

UPDATE #2
SET FechaVence = DATEADD(DAY, ISNULL(DiasPagado, 0), InicioVigencia)

UPDATE #2
SET DiasFaltante = DATEDIFF(DAY, GETDATE(), FechaVence)

SET @FechaVenceFinal = (SELECT FechaVence FROM #2 WHERE Ramo = 106 AND Poliza = ISNULL(@Poliza, Poliza))


DROP TABLE #1
DROP TABLE #2

set NOCOUNT ON

SELECT DISTINCT 
-- top 1000  

	b.NumeroCotizacion,
	a.[Compania] Company,
	a.[TipoFactura] InvoiceType,
/*
	ISNULL(Xy.Valor, 0) InvoiceAmount,
	ISNULL(Xy.ValorLey, 0) DPAAmount,
	ISNULL(Xy.ValorPropios, 0) DPAmount,
	ISNULL(Xy.ValorImpuestos, 0) TaxAmount,
	ISNULL(Xy.ValorServicio, 0) ServiceAmount,
	*/
	b.Poliza,
/*	b.Concepto,
--	xx.Descripcion,
 
	CASE
		WHEN flo IS NULL THEN 'NO' ELSE 'SI'
	END Enflotilla,
	*/ 
	@FechaVenceFinal  as  FechaInicioVigencia,
	getdate() as FechaFinVigencia,
	TipoVehiculo.DatoAlf VEHICLE_TYPE,

	/*ISNULL(Xy.ValorPropios, 0) DP_AMOUNT,
	cx.Descripcion AS UbicacionDescripcion,
	cx.DescripcionProvincia,
	cx.DescripcionMunicipio, */
	marca.DatoAlf MARCA,
	Modelo.DatoAlf Modelo,
	ano.DatoAlf Ano,
	color.DatoAlf AS Color,
	chasi.DatoAlf Chassis,
	placa.datoalf PLACA,
	bc.NombreCliente,
	bc.RNC as Cedula

	/*
	/*Colision y Vuelco*/
	ISNULL((SELECT TOP 1
		MontoInformativo
	FROM dbo.P_PolizaCoberturas AS a
	WHERE Cotizacion = c.Cotizacion AND a.Ramo = c.Ramo /*AND a.SubRamo = c.SubRamo*/ AND a.SecuenciaCot = TipoVehiculo.SecuenciaCot AND secuencia = 6), 0) AS SumaAsegurada,
	 c.Cotizacion,
	c.Ramo,
	c.SubRamo,
	TipoVehiculo.SecuenciaCot,	
    ramo.Descripcion as DescripcionRamo,
	SubRamo.Descripcion as DesripcionSubRamo ,
	isnull(pd.MontoAsegurado,0) as MontoAsegurado
	*/

FROM [dbo].[IN_DocumentoCobrarPrima] a
JOIN IN_DocumentoCobrar b
	ON a.Compania = b.Compania
	AND
	a.TipoFactura = b.TipoFactura
	AND
	a.Anulado = b.Anulado
	AND
	a.Sistema = b.Sistema
	AND
	a.Tipo = b.Tipo AND a.Numero = b.Numero
LEFT JOIN P_PolizaHeader X
	ON b.Poliza = x.Poliza --and b.NumeroCotizacion=x.Cotizacion
LEFT JOIN P_PolizaDetailmov Y
	ON x.Cotizacion = y.Cotizacion AND x.Ramo = Y.Ramo  

LEFT JOIN TipoPolizaSubRamo Z
	ON y.Ramo = z.ramo AND y.SubRamo = z.SubRamo
LEFT JOIN TipoPoliza XX
	ON z.CodigoTipoPoliza = xx.Codigo
LEFT JOIN (SELECT
	ramo,
	cotizacion,
	COUNT(*) Flo
FROM P_PolizaDetail
GROUP BY	ramo,
			cotizacion
HAVING COUNT(*) &gt; 1) flotillas
	ON flotillas.Cotizacion = y.Cotizacion AND y.Ramo = flotillas.Ramo

JOIN P_PolizaHeaderMov Bx
	ON b.NumeroCotizacion = bx.Cotizacion
	AND b.SecuenciaPoliza = bx.SecuenciaMov

INNER JOIN dbo.CXC_Clientes AS bc
	ON b.Compania = bc.Compania AND b.Cliente = bc.Codigo
LEFT OUTER JOIN dbo.UbicacionView AS cx
	ON ISNULL(bc.Ubicacion, 1) = cx.Codigo

JOIN P_PolizaDetailMov C
	ON bx.Cotizacion = c.cotizacion
	AND c.Compania = Bx.Compania
	AND bx.SecuenciaMov = c.SecuenciaMov
	AND c.Ramo = Y.Ramo AND C.SubRamo = y.subramo

LEFT JOIN [dbo].P_PolCondicionMov TipoVehiculo
	ON TipoVehiculo.Compania = C.compania
	AND TipoVehiculo.Cotizacion = C.Cotizacion
	AND C.Secuencia = TipoVehiculo.SecuenciaCot
	AND TipoVehiculo.SecuenciaCondicion = 1
	AND c.SecuenciaMov = TipoVehiculo.SecuenciaMov

LEFT JOIN IN_DocumentoCobrarPrimadetalle Xy
	ON b.Numero = Xy.Numero
	AND b.Tipo = Xy.Tipo
	AND b.Anulado = Xy.Anulado
	AND C.SubRamo = xy.Subramo
	AND xy.Item = c.Secuencia
	AND c.Ramo = xy.Ramo
	AND c.SecuenciaMov = Xy.SecuenciaMov
	AND c.Secuencia = xy.Item
	AND c.Cotizacion = Xy.Cotizacion
	AND b.NumeroCotizacion = Xy.Cotizacion
-- AND Y.SecuenciaMov = Xy.Item
--AND Y.Cotizacion = Xy.Cotizacion

LEFT JOIN [dbo].P_PolCondicionMov marca
	ON marca.Compania = C.compania
	AND marca.Cotizacion = C.Cotizacion
	AND C.Secuencia = marca.SecuenciaCot
	AND marca.SecuenciaCondicion = 2
	AND c.SecuenciaMov = marca.SecuenciaMov

LEFT JOIN [dbo].P_PolCondicionMov modelo
	ON modelo.Compania = C.compania
	AND modelo.Cotizacion = C.Cotizacion
	AND C.Secuencia = modelo.SecuenciaCot
	AND modelo.SecuenciaCondicion = 3
	AND c.SecuenciaMov = modelo.SecuenciaMov

LEFT JOIN [dbo].P_PolCondicionMov ano
	ON ano.Compania = C.compania
	AND ano.Cotizacion = C.Cotizacion
	AND C.Secuencia = ano.SecuenciaCot
	AND ano.SecuenciaCondicion = 4
	AND c.SecuenciaMov = ano.SecuenciaMov

LEFT JOIN [dbo].P_PolCondicionMov color
	ON color.Compania = C.compania
	AND color.Cotizacion = C.Cotizacion
	AND C.Secuencia = color.SecuenciaCot
	AND color.SecuenciaCondicion = 5
	AND c.SecuenciaMov = color.SecuenciaMov

LEFT JOIN [dbo].P_PolCondicionMov chasi
	ON chasi.Compania = C.compania
	AND chasi.Cotizacion = C.Cotizacion
	AND C.Secuencia = chasi.SecuenciaCot
	AND chasi.SecuenciaCondicion = 6
	AND c.SecuenciaMov = chasi.SecuenciaMov

LEFT JOIN [dbo].P_PolCondicionMov placa
	ON chasi.Compania = C.compania
	AND placa.Cotizacion = C.Cotizacion
	AND C.Secuencia = placa.SecuenciaCot
	AND placa.SecuenciaCondicion = 7
	AND c.SecuenciaMov = placa.SecuenciaMov
LEFT JOIN  P_Ramo ramo ON ramo.Ramo = z.ramo 
LEFT JOIN  P_SubRamo Subramo ON Subramo.Ramo = z.ramo AND Subramo.SubRamo = z.SubRamo
LEFT JOIN P_PolizaDetail  PD
	ON	PD.Cotizacion = y.Cotizacion AND 
		PD.Ramo = Y.Ramo and 
		pd.Ramo = Z.ramo  and 
		pd.SubRamo = Z.SubRamo    and PD.Secuencia  = c.Secuencia 
--LEFT JOIN @tmp1 TMP  ON  TMP.Poliza = b.Poliza
-- AND TMP.Ramo = X.RAMO 
  where b.Poliza =@Poliza</CommandText>
