<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

    @*@Styles.Render("~/Content/css")*@

    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />

    @Scripts.Render("~/bundles/modernizr")

    <link href="~/Content/select2-bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/select2.min.css" rel="stylesheet" />
    <link href="~/Content/dataTables.bootstrap4.min.css" rel="stylesheet" />

    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />

    <link href="~/Content/SecurityCss.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-1.12.4.min.js"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/dataTables.bootstrap4.min.js"></script>
    <link href="~/Content/atlantica-menu.css" rel="stylesheet" />
    <link href="~/Content/Custom.css" rel="stylesheet" />
</head>
@*<body class="pv_dtl">*@
<body>
    <div id="st-container" class="">
        @{Html.RenderPartial("_SlideMenuAgent"); }
        <div class="st-pusher">
            <div id="st-trigger-effects">
                <button data-effect="st-effect-2" id="btnLoadProfile" class="show_profile_es" onclick="GetUserProfileDataMVC(true);"> </button>
            </div>
            <div id="scroll_2" class="st-content-sv">
                @{ Html.RenderPartial("_TopMenu"); }
                <div class="clear"></div>
                <div class="st-content-st-sv">
                    @RenderBody()
                    <hr />
                    <footer>
                        @*<p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>*@
                    </footer>
                </div>
            </div>
        </div>

        <!--Modal-->
        @{Html.RenderPartial("_modalPopUp"); }
        <!--Modal-->
    </div>
    <script src="~/Scripts/select2.min.js"></script>
    @*<script src="~/Scripts/Security/jquery.nestedAccordion.js"></script>
        <script src="~/Scripts/Security/jquery.nicescroll.min.js"></script>
        <script src="~/Scripts/Security/jquery.notify.min.js"></script>
        <script src="~/Scripts/Security/jquery.countdownTimer.min.js"></script>*@
    <script src="~/Scripts/Utilities.js"></script>
    <script src="~/Scripts/AgentInfo.js"></script>
    <script src="~/Scripts/Custom.js"></script>

    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
