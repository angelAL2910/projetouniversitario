<div class="panel panel-default">
    <div class="panel-heading">
        <div class="row">
            <div class="col-lg-10 col-md-8 col-sm-12 col-xs-12 vendedores">
                <ul>
                    <li><h3 id="supervisorName" style="margin-bottom: 0; margin-top: 0;">@Html.Raw(ViewBag.SupervisorName) (@Html.Raw(ViewBag.SupervisorCode))</h3></li>
                    <li><h4 id="supervisorOffice" style="margin-bottom: 0; margin-top: 5px;">@Html.Raw(ViewBag.SupervisorOffices)</h4></li>
                    <li><h5 id="supervisorChannel" style="margin-bottom: 0; margin-top: 5px;">@Html.Raw(ViewBag.SupervisorChannel)</h5></li>
                    <li></li>
                </ul>
            </div>
            <div class="col-lg-2 col-md-4 col-sm-12 col-xs-12">
                <a id="btnOpenModal" type="button" class="btn btn-primary-st btn-block" href="@Url.Action("AddNewAgentInfo", "Agent", new { a="", newProspect="S" })">
                    <i class="fa fa-plus-circle" aria-hidden="true"></i>
                    Nuevo Prospecto
                </a>
            </div>
        </div>
    </div>
</div>
